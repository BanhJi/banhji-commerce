 class=\"pt-0\">\n                    <h2 class=\"mb-0 font_20\">{{ $t('transfer_orders') }}</h2>\n                </v-col>\n                <v-col sm=\"7\" cols=\"12\">\n<!--                    <v-row class=\"\">-->\n<!--                        <v-col class=\"py-0\" sm=\"6\" cols=\"12\">-->\n<!--                            <v-card outlined dense class=\"px-2 no_border\" color=\"secondary\" min-height=\"53px\">-->\n<!--                                <v-row class=\"\">-->\n<!--                                    <v-col class=\"py-1\" sm=\"6\" cols=\"12\">-->\n<!--                                        <p class=\"white&#45;&#45;text mb-0 text-uppercase\">{{ $t('incoming_transfer') }}</p>-->\n<!--                                    </v-col>-->\n<!--                                    <v-col class=\"py-1\" sm=\"6\" cols=\"12\">-->\n<!--                                        <h3 class=\"white&#45;&#45;text mt-3 text-right\">100</h3>-->\n<!--                                    </v-col>-->\n<!--                                </v-row>-->\n<!--                            </v-card>-->\n<!--                        </v-col>-->\n<!--                        <v-col class=\"py-0\" sm=\"6\" cols=\"12\">-->\n<!--                            <v-card outlined dense class=\"px-2 no_border\" color=\"third\" min-height=\"53px\">-->\n<!--                                <v-row class=\"\">-->\n<!--                                    <v-col class=\"py-1\" sm=\"6\" cols=\"12\">-->\n<!--                                        <p class=\"white&#45;&#45;text mb-0 text-uppercase\">{{ $t('ready_for_transfer') }}</p>-->\n<!--                                    </v-col>-->\n<!--                                    <v-col class=\"py-1\" sm=\"6\" cols=\"12\">-->\n<!--                                        <h3 class=\"white&#45;&#45;text mt-3 text-right\">100</h3>-->\n<!--                                    </v-col>-->\n<!--                                </v-row>-->\n<!--                            </v-card>-->\n<!--                        </v-col>-->\n<!--                    </v-row>-->\n                </v-col>\n            </v-row>\n            <v-row class=\"\">\n                <v-col sm=\"9\" cols=\"12\" class=\"pt-0\">\n                    <v-row class=\"\">\n                        <v-col sm=\"4\" cols=\"12\" class=\"py-0\">\n                            <v-select class=\"mt-1\"\n                                      :items=\"dateSorters\"\n                                      @change=\"onSorterChanges\"\n                                      clearable\n                                      outlined\n                                      placeholder=\"ALL\"\n                            />\n                        </v-col>\n\n                        <v-col sm=\"3\" cols=\"12\" class=\"py-0\">\n                            <app-datepicker :initialDate=\"start_date\" @emitDate=\"start_date = $event\"/>\n                        </v-col>\n\n                        <v-col sm=\"3\" cols=\"12\" class=\"py-0\">\n                            <app-datepicker :initialDate=\"end_date\" @emitDate=\"end_date = $event\"/>\n                        </v-col>\n\n                        <v-col sm=\"1\" cols=\"1\" class=\"pt-1\">\n                            <v-btn class=\"btn_search\" @click=\"searchTransaction\" style=\"background-color: rgb(237, 241, 245)\">\n                                <i class=\"b-search\" style=\"font-size: 18px; color:#fff !important;\"/>\n                            </v-btn>\n                        </v-col>\n                    </v-row>\n                </v-col>\n                <v-col sm=\"3\" cols=\"12\" class=\"pt-1\">\n                    <v-btn to=\"transfer_order\" color=\"primary\" class=\" white--text float-right text-capitalize\">\n                        {{$t('new_transfer')}}\n                    </v-btn>\n                </v-col>\n            </v-row>\n\n            <v-row class=\"\">\n                <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                    <template>\n                        <v-simple-table class=\"attachment_table\">\n                            <template v-slot:default>\n                                <thead>\n                                <tr>\n                                    <th>{{ $t('date') }}</th>\n                                    <th>{{ $t('number') }}</th>\n                                    <th>{{ $t('from') }}</th>\n                                    <th>{{ $t('to') }}</th>\n                                    <th>{{ $t('status') }}</th>\n                                    <th>{{ $t('action') }}</th>\n                                </tr>\n                                </thead>\n                                <tbody>\n                                <tr v-for=\"to in transferOrders\" v-bind:key=\"to.id\">\n                                    <td>{{ new Date(to.date).toISOString().substr(0, 10) }}</td>\n                                    <td>{{ to.number }}</td>\n                                    <td>{{ to.fWarehouse ? to.fWarehouse.name : '' }}</td>\n                                    <td>{{ to.tWarehouse ? to.tWarehouse.name : '' }}</td>\n                                    <td></td>\n                                    <td class=\"text-center\">\n                                        <v-btn class=\"bg-none\">\n                                            <v-icon size=\"17px\"  class=\"primary--text\">fa fa-eye</v-icon>\n                                        </v-btn>\n                                    </td>\n                                </tr>\n                                </tbody>\n                            </template>\n                        </v-simple-table>\n                    </template>\n                </v-col>\n            </v-row>\n        </v-col>\n    </v-row>\n</template>\n<script>\nimport DatePickerComponent from '@/components/custom_templates/DatePickerComponent'\nimport kendo from \"@progress/kendo-ui\";\n\nconst transferOrderHandler = require(\"@/scripts/transferOrderHandler\")\nexport default {\n    data: () => ({\n        // Search transaction dates\n        start_date: \"\",\n        end_date: \"\",\n        dateSorters: ['Today', 'This Week', 'This Month', 'This Year'],\n        // LoadingMe\n        isLoaded: false,\n        // Kendo dataSource\n        schemaDefinition: {\n            model: {id: \"id\"}\n        },\n        /* Sort by account number */\n        sortDefinition: {field: \"number\"},\n\n        transferOrders: []\n    }),\n    props: {},\n    methods: {\n        async onloadTransferOrder() {\n            await transferOrderHandler.getAll().then(res => {\n                this.transferOrders = res\n                window.console.log(res)\n            })\n        },\n        // On Sorter Changes\n        onSorterChanges(val) {\n            let today = new Date()\n\n            switch (val) {\n                case \"Today\":\n                    this.start_date = kendo.toString(today, 'yyyy-MM-dd')\n                    this.end_date = kendo.toString(today, 'yyyy-MM-dd')\n\n                    break\n                case \"This Week\":\n                    var first = today.getDate() - today.getDay(),\n                        last = first + 6;\n\n                    this.start_date = kendo.toString(new Date(today.setDate(first)), 'yyyy-MM-dd')\n                    this.end_date = kendo.toString(new Date(today.setDate(last)), 'yyyy-MM-dd')\n\n                    break\n                case \"This Month\":\n                    this.start_date = kendo.toString(new Date(today.getFullYear(), today.getMonth(), 1), 'yyyy-MM-dd')\n                    this.end_date = kendo.toString(new Date(today.getFullYear(), today.getMonth(), 31), 'yyyy-MM-dd')\n\n                    break\n                case \"This Year\":\n                    this.start_date = kendo.toString(new Date(today.getFullYear(), 0, 1), 'yyyy-MM-dd')\n                    this.end_date = kendo.toString(new Date(today.getFullYear(), 11, 31), 'yyyy-MM-dd')\n\n                    break\n                default:\n                    this.start_date = \"\";\n                    this.end_date = \"\";\n            }\n        },\n        // Search Transaction\n        searchTransaction() {\n            let data = {\n                startDate: this.start_date,\n                endDate: this.end_date\n            }\n            transferOrderHandler.getAll(data).then(res => {\n                this.transferOrders = res\n                window.console.log(res)\n            })\n        },\n    },\n    async activated() {\n        await this.onloadTransferOrder()\n    },\n    async mounted() {\n        await this.onloadTransferOrder()\n    },\n    computed: {},\n    components: {\n        'app-datepicker': DatePickerComponent,\n    },\n};\n</script>\n<style scoped>\n.acc_group header {\n    height: 10px !important;\n}\n\ntable.acc_group tr td {\n    border-bottom: 1px solid #ccc;\n    padding: 8px;\n    vertical-align: middle;\n}\n\ntable.acc_group tr td:first-child {\n    font-weight: 700;\n}\n\ntable.acc_group tr td:last-child {\n    text-align: center;\n}\n\n.v-card__actions .v-btn.v-btn {\n    padding: 0 16px;\n}\n\n.exchange-table.theme--light.v-data-table > .v-data-table__wrapper > table > thead > tr > th {\n    font-family: 'Niradei-Bold', serif;\n    color: #000 !important;\n    border-top: 1px solid #000 !important;\n    border-bottom: 1px solid #000 !important;\n    font-size: 15px !important;\n}\n\n.exchange-table.theme--light.v-data-table > .v-data-table__wrapper > table > thead > tr > td {\n    color: #000 !important;\n    padding: 5px !important;\n}\n\n.exchange-table.theme--light.v-data-table > .v-data-table__wrapper > table > tbody > tr:last-child td {\n    border-bottom: 1px solid #000 !important;\n}\n\n.function_footer {\n    padding: 15px;\n    display: inline-block;\n}\n\np {\n    color: rgba(0, 0, 0, 0.87);\n}\n\n.actionBtn {\n    height: 47.2px !important;\n}\n\n.text_tip {\n    font-size: 9px;\n    line-height: 10px;\n}\n\n@media (max-width: 576px) {\n\n}\n</style>"]}]}