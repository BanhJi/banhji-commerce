-row>\n        <v-col sm=\"12\" cols=\"12\" class=\"pt-0\">\n            <h2 class=\"mb-0 font_20\">{{ $t(\"brand\") }}</h2>\n            <v-dialog v-model=\"dialogM2\" max-width=\"450px\">\n                <template v-slot:activator=\"{ on }\">\n                    <v-btn\n                        color=\"primary\"\n                        class=\" text-capitalize white--text float-right\"\n                        v-on=\"on\"\n                        @click=\"onNewClick\"\n                    >\n                        {{ $t(\"create_new\") }}\n                    </v-btn>\n                </template>\n                <v-card>\n                    <div class=\"modal_header\">\n                        <v-card-title>{{ $t(\"brand\") }}</v-card-title>\n                        <v-icon class=\"\" @click=\"dialogM2 = false\">close</v-icon>\n                    </div>\n                    <v-card-text class=\"modal_text_content\">\n                        <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n                            <v-row>\n                                <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                                    <v-row>\n                                        <v-col sm=\"4\" cols=\"4\">\n                                            <label class=\"label\">{{ $t(\"name\") }}</label>\n                                            <v-text-field\n                                                class=\"mt-1\"\n                                                v-model=\"brand.name\"\n                                                tag=\"brand.name\"\n                                                outlined\n                                                :rules=\"[\n                          (v) => !!v || $t('name_is_required'),\n                          (v) => (v && v.length > 2) || $t('character_3'),\n                        ]\"\n                                                required\n                                                placeholder=\"\"\n                                            />\n                                        </v-col>\n                                        <v-col sm=\"4\" cols=\"4\" class=\"pb-0\">\n                                            <label class=\"label\">{{ $t(\"code\") }}</label>\n                                            <v-text-field\n                                                class=\"mt-1\"\n                                                v-model=\"brand.code\"\n                                                tag=\"brand.code\"\n                                                outlined\n                                                :rules=\"[\n                          (v) => !!v || $t('code_is_required'),\n                          (v) => (v && v.length > 2) || $t('character_3'),\n                        ]\"\n                                                required\n                                                placeholder=\"\"\n                                            />\n                                        </v-col>\n                                        <v-col sm=\"4\" cols=\"4\" class=\"pb-0\">\n                                            <label class=\"label\">{{ $t(\"abbr\") }}</label>\n                                            <v-text-field\n                                                class=\"mt-1\"\n                                                v-model=\"brand.abbr\"\n                                                tag=\"brand.abbr\"\n                                                outlined\n                                                :rules=\"[(v) => !!v || $t('abbr_is_required')]\"\n                                                required\n                                                placeholder=\"\"\n                                            />\n                                        </v-col>\n                                    </v-row>\n                                </v-col>\n                            </v-row>\n                        </v-form>\n                    </v-card-text>\n                    <v-divider/>\n                    <v-card-actions class=\"modal_footer\">\n                        <v-row>\n                            <v-col sm=\"6\" cols=\"6\" class=\"py-0 text-left\">\n                                <v-btn\n                                    color=\"black\"\n                                    outlined\n                                    class=\" text-capitalize  black--text float-left\"\n                                    @click=\"dialogM2 = false\"\n                                >{{ $t(\"cancel\") }}\n                                </v-btn>\n                            </v-col>\n                            <v-col sm=\"6\" cols=\"6\" class=\"py-0 text-right\">\n                                <v-btn\n                                    color=\"primary\"\n                                    class=\"px-3  white--text text-capitalize\"\n                                    :disabled=\"btnDisabled\"\n                                    @click=\"onSaveClose\">\n                                    {{ $t(\"save_close\") }}\n                                </v-btn>\n                            </v-col>\n                        </v-row>\n                    </v-card-actions>\n                </v-card>\n            </v-dialog>\n            <p class=\"mb-0\">{{ $t(\"brand_desc\") }}</p>\n        </v-col>\n\n        <v-col sm=\"12\" cols=\"12\">\n            <template>\n                <LoadingMe\n                    :isLoading=\"showLoading\"\n                    :fullPage=\"false\"\n                    type=\"loading\"\n                    :myLoading=\"true\"\n                />\n                <kendo-datasource ref=\"productBrandDS\" :data=\"brands\"/>\n                <kendo-grid\n                    id=\"gridProductBrand\"\n                    class=\"grid-function\"\n                    :data-source-ref=\"'productBrandDS'\"\n                    :editable=\"false\"\n                    :toolbar=\"toolbarTemplate\"\n                    :excel-export=\"excelExport\"\n                    :excel-file-name=\"'BrandList.xlsx'\"\n                    :excel-filterable=\"true\"\n                    :scrollable-virtual=\"true\">\n                    <kendo-grid-column\n                        :field=\"'no'\"\n                        :title=\"$t('no')\"\n                        :template=\"rowNumberTmpl\"\n                        :width=\"40\"\n                        :column-menu=\"false\"\n                        :headerAttributes=\"{ style: 'background-color: #EDF1F5;', class: 'text-center', }\"\n                        :attributes=\"{ style: 'text-align: center' }\"\n                    />\n                    <kendo-grid-column\n                        :field=\"'abbr'\"\n                        :title=\"$t('abbr')\"\n                        :width=\"50\"\n                        :template=\"'<span>#=abbr || ``#</span>'\"\n                        :headerAttributes=\"{ style: 'background-color: #EDF1F5' }\"\n                    />\n                    <kendo-grid-column\n                        :field=\"'code'\"\n                        :title=\"$t('code')\"\n                        :width=\"50\"\n                        :template=\"'<span>#=code#</span>'\"\n                        :headerAttributes=\"{ style: 'background-color: #EDF1F5' }\"\n                    />\n                    <kendo-grid-column\n                        :field=\"'name'\"\n                        :attributes=\"{ class: 'tb_name_td' }\"\n                        :title=\"$t('name')\"\n                        :width=\"90\"\n                        :template=\"'<span>#=name#</span>'\"\n                        :headerAttributes=\"{ style: 'background-color: #EDF1F5' }\"\n                    />\n                    <kendo-grid-column\n                        :field=\"''\"\n                        :title=\"$t('action')\"\n                        :width=\"70\"\n                        :command=\"[{ iconClass: 'k-icon k-i-edit', text: 'Edit', click: goEdit },]\"\n                        :headerAttributes=\"{ style: 'text-align: left; background-color: #EDF1F5', }\"\n                    />\n                </kendo-grid>\n            </template>\n        </v-col>\n    </v-row>\n</template>\n\n<script>\nimport kendo from \"@progress/kendo-ui\";\n\nconst brandHandler = require(\"@/scripts/brandHandler\");\n\nconst $ = kendo.jQuery\nimport JSZip from \"jszip\";\n\nwindow.JSZip = JSZip;\n\nexport default {\n    components: {\n        LoadingMe: () => import(`@/components/Loading`),\n    },\n    data: () => ({\n        dialogM2: false,\n        showLoading: true,\n        valid: true,\n        brand: {\n            id: \"\",\n            name: \"\",\n            code: \"\",\n            abbr: \"\",\n        },\n        brands: [],\n        option: 1,\n        btnDisabled: false\n    }),\n    props: {},\n    computed: {},\n    watch: {\n        dialogm2(val) {\n            val || this.close();\n        },\n    },\n    created() {\n    },\n    methods: {\n        goEdit(e) {\n            e.preventDefault();\n            let grid = kendo.jQuery(\"#gridProductBrand\").data(\"kendoGrid\");\n            let dataItem = grid.dataItem($(e.currentTarget).closest(\"tr\"));\n            this.brand = dataItem;\n            this.dialogM2 = true;\n        },\n        onNewClick() {\n            this.btnDisabled = false\n            this.brand.id = \"\";\n            this.clear();\n        },\n        close() {\n            this.dialogM2 = false;\n        },\n        clear() {\n            this.brand = {};\n        },\n        async onSaveClose() {\n            if (!this.$refs.form.validate()) {\n                this.$refs.form.validate();\n                return;\n            }\n            new Promise((resolve) => {\n                setTimeout(() => {\n                    resolve(\"resolved\");\n                    if (this.brand.id === \"\" || this.brand.id === undefined) {\n                        let data = {\n                            name: this.brand.name,\n                            code: this.brand.code,\n                            abbr: this.brand.abbr,\n                        };\n                        this.showLoading = true\n                        this.btnDisabled = true\n                        brandHandler\n                            .create(data)\n                            .then((response) => {\n                                if (response.data.statusCode === 201) {\n                                    this.showLoading = false\n                                    this.btnDisabled = false\n                                    this.loadBrand();\n                                    this.clear();\n                                    this.close();\n                                    this.$refs.form.reset();\n                                } else {\n                                    this.showLoading = false\n                                    this.btnDisabled = false\n                                }\n                            })\n                            .catch((e) => {\n                                this.showLoading = false\n                                this.btnDisabled = false\n                                this.errors.push(e);\n                            });\n                    } else {\n                        let data = {\n                            name: this.brand.name,\n                            code: this.brand.code,\n                            abbr: this.brand.abbr,\n                        };\n                        brandHandler\n                            .update(this.brand.id, data)\n                            .then((response) => {\n                                if (response.data.statusCode === 201) {\n                                    this.loadBrand();\n                                    this.clear();\n                                    this.close();\n                                    this.$refs.form.reset();\n                                }\n                            })\n                            .catch((e) => {\n                                this.errors.push(e);\n                            });\n                    }\n                }, 10);\n            });\n        },\n        async loadBrand() {\n            new Promise((resolve) => {\n                setTimeout(() => {\n                    resolve(\"resolved\");\n                    this.showLoading = true\n                    brandHandler.get().then((res) => {\n                        this.showLoading = false;\n                        this.brands = res;\n                    });\n                }, 10);\n            });\n        },\n\n        toolbarTemplate: function () {\n            const templateHtml =\n                '<span>' +\n                '<a class=\"k-pager-excel k-link k-button excel\" title=\"Export to Excel\"> <span class=\"k-icon k-i-excel\"></span>Export to excel </a>' +\n                '</span><span>&nbsp;&nbsp;</span>' +\n                '<span style=\"position:absolute; right:5px\">' +\n                '</span>' +\n                '<span>' +\n                '<a class=\"k-pager-refresh k-link k-button reload\" title=\"Refresh\"><span class=\"k-icon k-i-reload\"></span> Refresh </a>' +\n                '</span>'\n\n            return kendo.template(templateHtml)\n        },\n        exportToExcel() {\n            const grid = $(\"#gridProductBrand\").data(\"kendoGrid\")\n            grid.saveAsExcel()\n        },\n        excelExport: function (e) {\n            // const pivot = this.$refs.gridTransactionDS.kendoWidget()\n            // pivot.saveAsExcel()\n            window.console.log(e.data);\n        },\n        initToolbar(that) {\n            let grid = $(\"#gridProductBrand\").data(\"kendoGrid\");\n            let gridElement = grid.element;\n            let toolbarElement = gridElement.find(\".k-grid-toolbar\");\n            toolbarElement.on(\"click\", \".k-pager-refresh\", function (e) {\n                e.preventDefault();\n                onReloadData(that);\n            });\n            toolbarElement.on(\"click\", \".k-pager-excel\", function (e) {\n                e.preventDefault();\n                onExportExcel(that);\n            });\n\n            function onReloadData(that) {\n                that.loadBrand();\n            }\n\n            function onExportExcel(that) {\n                that.exportToExcel();\n            }\n        },\n        rowNumberTmpl(dataItem) {\n            let ds = this.$refs.productBrandDS.kendoWidget(),\n                index = ds.indexOf(dataItem);\n            return index + 1;\n        },\n    },\n    mounted: async function () {\n        await this.loadBrand();\n        await this.initToolbar(this);\n    },\n};\n</script>\n<style scoped>\n.acc_group header {\n    height: 10px !important;\n}\n\ntable.acc_group tr td {\n    border-bottom: 1px solid #ccc;\n    padding: 8px;\n    vertical-align: middle;\n}\n\ntable.acc_group tr td:first-child {\n    font-weight: 700;\n}\n\ntable.acc_group tr td:last-child {\n    text-align: center;\n}\n\n.v-card__actions .v-btn.v-btn {\n    padding: 0 16px;\n}\n\n.exchange-table.theme--light.v-data-table\n> .v-data-table__wrapper\n> table\n> thead\n> tr\n> th {\n    font-family: \"Niradei-Bold\", serif;\n    color: #000 !important;\n    border-top: 1px solid #000 !important;\n    border-bottom: 1px solid #000 !important;\n    font-size: 15px !important;\n}\n\n.exchange-table.theme--light.v-data-table\n> .v-data-table__wrapper\n> table\n> thead\n> tr\n> td {\n    color: #000 !important;\n    padding: 5px !important;\n}\n\n.exchange-table.theme--light.v-data-table\n> .v-data-table__wrapper\n> table\n> tbody\n> tr:last-child\ntd {\n    border-bottom: 1px solid #000 !important;\n}\n\n.function_footer {\n    padding: 15px;\n    display: inline-block;\n}\n\np {\n    color: rgba(0, 0, 0, 0.87);\n}\n\n.actionBtn {\n    height: 47.2px !important;\n}\n\n.text_tip {\n    font-size: 9px;\n    line-height: 10px;\n}\n\n@media (max-width: 576px) {\n}\n</style>\n"]}]}