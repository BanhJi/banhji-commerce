return-object\n      outlined\n    >\n      <template slot=\"selection\" slot-scope=\"{ item }\">\n        {{ Helper.formatFiscalDate(item.start_date) }} -\n        {{ Helper.formatFiscalDate(item.end_date) }}\n      </template>\n      <template slot=\"item\" slot-scope=\"{ item }\">\n        {{ Helper.formatFiscalDate(item.start_date) }} -\n        {{ Helper.formatFiscalDate(item.end_date) }}\n      </template>\n    </v-select>\n  </div>\n</template>\n\n<script>\nimport Helper from \"@/helper.js\";\n\n/* Store */\nimport store from \"@/store\";\nconst institute = store.state.institute.institute;\n\nexport default {\n  name: \"FiscalDateDropDownList\",\n  props: {\n    initFiscalDate: {\n      type: Object,\n    },\n  },\n  data() {\n    return {\n      Helper: Helper,\n      fiscalDate: institute.fiscalDate[0],\n      fiscalDates: institute.fiscalDate,\n    };\n  },\n  methods: {\n    emitFiscalDate() {\n      this.$emit(\"emitFiscalDate\", this.fiscalDate);\n    },\n    onChanged() {\n      this.$emit(\"onChanged\");\n    },\n  },\n  watch: {\n    initFiscalDate() {\n      this.fiscalDate = this.initFiscalDate;\n    },\n  },\n  mounted() {\n    /* Initial Fiscal Date */\n    this.fiscalDate = this.initFiscalDate;\n  },\n};\n</script>\n<style scoped>\n.v-select__selections input {\n  display: none !important;\n}\n</style>\n"]}]}