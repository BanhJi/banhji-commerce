\n            <v-dialog v-model=\"dialogM2\" max-width=\"400px\">\n                <template v-slot:activator=\"{ on }\">\n                    <v-btn color=\"primary\" class=\" text-capitalize white--text float-right\" v-on=\"on\"\n                           @click=\"onNewClick\">\n                        {{ $t('create_new') }}\n                    </v-btn>\n                </template>\n                <v-card>\n                    <div class=\"modal_header\">\n                        <v-card-title>{{ $t('uom_full') }}</v-card-title>\n                        <v-icon class=\"btn_close\" @click=\"dialogM2 = false\">close</v-icon>\n                    </div>\n                    <v-card-text class=\"modal_text_content\">\n                        <v-form\n                            ref=\"form\"\n                            v-model=\"valid\"\n                            lazy-validation\n                        >\n                            <v-row>\n                                <v-col sm=\"12\" cols=\"12\" class=\"pb-0\">\n                                    <label class=\"label\">{{ $t('category') }}</label>\n                                    <v-select class=\"mt-1\"\n                                              tage=\"Category\"\n                                              v-model=\"mUOMCategory\"\n                                              :items=\"uomCategories\"\n                                              item-value=\"id\"\n                                              item-text=\"name\"\n                                              :rules=\"[v => !!v || $t('category_is_required')]\"\n                                              required\n                                              return-object\n                                              outlined/>\n                                    <label class=\"label\">{{ $t('name') }}</label>\n                                    <v-text-field class=\"mt-1\"\n                                                  v-model=\"name\"\n                                                  outlined\n                                                  :rules=\"[v => !!v || $t('name_is_required'),v => (v && v.length > 2) || $t('character_3')]\"\n                                                  required\n                                                  placeholder=\"\"/>\n                                </v-col>\n                            </v-row>\n                        </v-form>\n                    </v-card-text>\n                    <v-divider/>\n                    <v-card-actions class=\"modal_footer\">\n                        <v-row>\n                            <v-col sm=\"6\" cols=\"6\" class=\"py-0 text-left\">\n                                <v-btn color=\"primary\" outlined\n                                       class=\" text-capitalize  black--text float-left\"\n                                       @click=\"dialogM2 = false\">{{ $t('cancel') }}\n                                </v-btn>\n                            </v-col>\n                            <v-col sm=\"6\" cols=\"6\" class=\"py-0 text-right\">\n                                <v-btn color=\"primary\" class=\"px-3  white--text text-capitalize\"\n                                       @click=\"onSaveClose\">{{ $t('save_close') }}\n                                </v-btn>\n                            </v-col>\n                        </v-row>\n                    </v-card-actions>\n\n                </v-card>\n            </v-dialog>\n            <p class=\"mb-0\">{{ $t('categories_desc') }} </p>\n        </v-col>\n\n\n        <v-col sm=\"12\" cols=\"12\">\n            <template>\n                <LoadingMe\n                    :isLoading=\"showLoading\"\n                    :fullPage=\"false\"\n                    type=\"loading\"\n                    :myLoading=\"true\"\n                />\n                <kendo-datasource\n                    ref=\"uomDS\"\n                    :group=\"group\"\n                    :data=\"uoms\"/>\n                <kendo-grid\n                    id=\"gridUOM\"\n                    class=\"grid-function\"\n                    :data-source-ref=\"'uomDS'\"\n                    :editable=\"false\"\n                    :resizable=\"true\"\n                    :toolbar=\"toolbarTemplate\"\n                    :excel-export=\"excelExport\"\n                    :excel-file-name=\"'UOMList.xlsx'\"\n                    :excel-filterable=\"true\"\n                    :scrollable-virtual=\"true\">\n                    <!--                    <kendo-grid-column-->\n                    <!--                        :field=\"'no'\"-->\n                    <!--                        :title=\"$t('no')\"-->\n                    <!--                        :template=\"rowNumberTmpl\"-->\n                    <!--                        :width=\"40\"-->\n                    <!--                        :column-menu=\"false\"-->\n                    <!--                        :headerAttributes=\"{ style: 'background-color: #EDF1F5;', class: 'text-center'\t}\"-->\n                    <!--                        :attributes=\"{style: 'text-align: center'}\"/>-->\n                    <kendo-grid-column\n                        :field=\"'name'\"\n                        :title=\"$t('name')\"\n                        :attributes=\"{ class: 'tb_name_td' }\"\n                        :width=\"150\"\n                        :template=\"'<span>#=name#</span>'\"\n                        :headerAttributes=\"{ style: 'background-color: #EDF1F5' }\"\n                    />\n                    <kendo-grid-column\n                        :field=\"'category.name'\"\n                        :title=\"$t('category')\"\n                        :attributes=\"{ class: 'tb_name_td' }\"\n                        :width=\"150\"\n                        :headerAttributes=\"{ style: 'background-color: #EDF1F5' }\"\n                    />\n                    <kendo-grid-column\n                        :field=\"''\"\n                        :title=\"$t('action')\"\n                        :width=\"84\"\n                        :command=\"[{ iconClass: 'k-icon k-i-edit', text: ' ', click: goEdit },]\"\n                        :headerAttributes=\"{\n              style: 'text-align: left; background-color: #EDF1F5',\n            }\"\n                    />\n                </kendo-grid>\n            </template>\n        </v-col>\n    </v-row>\n</template>\n\n<script>\nimport kendo from \"@progress/kendo-ui\";\n\nconst uomCategoryHandler = require(\"@/scripts/uomCategoryHandler\")\nconst uomHandler = require(\"@/scripts/uomHandler\")\nconst $ = kendo.jQuery;\n\nimport JSZip from \"jszip\";\n\nwindow.JSZip = JSZip;\nexport default {\n    components: {\n        LoadingMe: () => import(`@/components/Loading`)\n    },\n    data: () => ({\n        dialogM2: false,\n        showLoading: true,\n        valid: true,\n        name: '',\n        uomId: '',\n        uoms: [],\n        uocId: '',\n        mUOMCategory: '',\n        uomCategories: [],\n        option: 1,\n        group: {field: 'category.name'},\n    }),\n    props: {},\n    computed: {},\n    watch: {\n        dialogM2(val) {\n            val || this.close()\n        },\n    },\n    created() {\n    },\n    methods: {\n        goEdit(e) {\n            this.loadUOC()\n            e.preventDefault();\n            let grid = kendo.jQuery(\"#gridUOM\").data(\"kendoGrid\");\n            const dataItem = grid.dataItem($(e.currentTarget).closest(\"tr\"));\n            // window.console.log(dataItem, 'dataItem')\n            this.name = dataItem.name || ''\n            this.uomId = dataItem.id || ''\n            this.mUOMCategory = dataItem.category || {}\n            this.uocId = dataItem.category.id\n            this.dialogM2 = true\n        },\n        rowNumberTmpl(dataItem) {\n            let ds = this.$refs.uomDS.kendoWidget(),\n                index = ds.indexOf(dataItem);\n            return index + 1;\n        },\n        onNewClick() {\n            this.uomId = ''\n            this.clear()\n            this.loadUOC()\n        },\n        close() {\n            this.dialogM2 = false\n        },\n        clear() {\n            this.name = ''\n\n        },\n        async onSaveClose() {\n            if (!this.$refs.form.validate()) {\n                this.$refs.form.validate()\n                return\n            }\n            new Promise(resolve => {\n                setTimeout(() => {\n                    resolve('resolved');\n                    if (this.uomId === '') {\n                        let data = {\n                            \"name\": this.name,\n                            \"category\": this.mUOMCategory\n                        }\n                        uomHandler.create(data).then(response => {\n                            if (response.data.statusCode === 201) {\n                                this.loadUOM()\n                                this.clear()\n                                this.close()\n                                this.$refs.form.resetValidation()\n                                this.$refs.form.reset()\n                            }\n                        }).catch(e => {\n                            this.errors.push(e)\n                        })\n                    } else {\n                        let isCategoryChanged = 1;\n                        if (this.uocId === this.mUOMCategory.id) {\n                            isCategoryChanged = 0\n                        }\n                        const category = this.mUOMCategory || {}\n                        let data = {\n                            \"name\": this.name,\n                            \"category\": {\n                                id: category.id,\n                                name: category.name || '',\n                                isSystem: category.isSystem,\n                            },\n                            \"isCategoryChanged\": isCategoryChanged,\n                            \"delCategoryId\": this.uocId\n                        }\n                        uomHandler.update(this.uomId, data).then(response => {\n                            if (response.data.statusCode === 201) {\n                                this.loadUOM()\n                                this.clear()\n                                this.close()\n                                this.$refs.form.reset()\n                                this.$refs.form.resetValidation()\n                            }\n                        }).catch(e => {\n                            this.errors.push(e)\n                        })\n                    }\n                }, 10);\n            })\n        },\n        async loadUOC() {\n            new Promise(resolve => {\n                setTimeout(() => {\n                    this.showLoading = true\n                    resolve('resolved');\n                    this.showLoading = false\n                    uomCategoryHandler.get().then(res => {\n                        this.uomCategories = res\n                    })\n                }, 10);\n            })\n        },\n        async loadUOM() {\n            new Promise(resolve => {\n                setTimeout(() => {\n                    resolve('resolved');\n                    this.showLoading = true\n                    uomHandler.get().then(res => {\n                        this.showLoading = false\n                        this.uoms = res\n                    })\n                }, 10);\n            })\n        },\n\n        excelExport: function (e) {\n            // const pivot = this.$refs.gridTransactionDS.kendoWidget()\n            // pivot.saveAsExcel()\n            window.console.log(e.data);\n        },\n        initToolbar(that) {\n            let grid = $(\"#gridUOM\").data(\"kendoGrid\");\n            let gridElement = grid.element;\n            let toolbarElement = gridElement.find(\".k-grid-toolbar\");\n            toolbarElement.on(\"click\", \".k-pager-refresh\", function (e) {\n                e.preventDefault();\n                onReloadData(that);\n            });\n            toolbarElement.on(\"click\", \".k-pager-excel\", function (e) {\n                e.preventDefault();\n                onExportExcel(that);\n            });\n\n            function onReloadData(that) {\n                that.loadUOM();\n            }\n\n            function onExportExcel(that) {\n                that.exportToExcel();\n            }\n        },\n        toolbarTemplate: function () {\n            const templateHtml =\n                '<span>' +\n                '<a class=\"k-pager-excel k-link k-button excel\" title=\"Export to Excel\"> <span class=\"k-icon k-i-excel\"></span>Export to excel </a>' +\n                '</span><span>&nbsp;&nbsp;</span>' +\n                '<span style=\"position:absolute; right:5px\">' +\n                '</span>' +\n                '<span>' +\n                '<a class=\"k-pager-refresh k-link k-button reload\" title=\"Refresh\"><span class=\"k-icon k-i-reload\"></span> Refresh </a>' +\n                '</span>'\n\n            return kendo.template(templateHtml)\n        },\n        exportToExcel() {\n            const grid = $(\"#gridUOM\").data(\"kendoGrid\")\n            grid.saveAsExcel()\n        },\n    },\n    mounted: async function () {\n        await this.loadUOC()\n        await this.loadUOM()\n        await this.initToolbar(this)\n    }\n};\n</script>\n<style scoped>\n.acc_group header {\n    height: 10px !important;\n}\n\ntable.acc_group tr td {\n    border-bottom: 1px solid #ccc;\n    padding: 8px;\n    vertical-align: middle;\n}\n\ntable.acc_group tr td:first-child {\n    font-weight: 700;\n}\n\ntable.acc_group tr td:last-child {\n    text-align: center;\n}\n\n.v-card__actions .v-btn.v-btn {\n    padding: 0 16px;\n}\n\n.exchange-table.theme--light.v-data-table > .v-data-table__wrapper > table > thead > tr > th {\n    font-family: 'Niradei-Bold', serif;\n    color: #000 !important;\n    border-top: 1px solid #000 !important;\n    border-bottom: 1px solid #000 !important;\n    font-size: 15px !important;\n}\n\n.exchange-table.theme--light.v-data-table > .v-data-table__wrapper > table > thead > tr > td {\n    color: #000 !important;\n    padding: 5px !important;\n}\n\n.exchange-table.theme--light.v-data-table > .v-data-table__wrapper > table > tbody > tr:last-child td {\n    border-bottom: 1px solid #000 !important;\n}\n\n.function_footer {\n    padding: 15px;\n    display: inline-block;\n}\n\np {\n    color: rgba(0, 0, 0, 0.87);\n}\n\n.actionBtn {\n    height: 47.2px !important;\n}\n\n.text_tip {\n    font-size: 9px;\n    line-height: 10px;\n}\n\n@media (max-width: 576px) {\n\n}\n</style>"]}]}