                              </v-btn>\n                                                    </v-col>\n\n                                                    <v-col sm=\"12\" cols=\"12\" class=\"\">\n                                                        <template>\n                                                            <v-simple-table class=\"attachment_table\">\n                                                                <template v-slot:default>\n                                                                    <thead>\n                                                                    <tr>\n                                                                        <th>{{ $t('date') }}</th>\n                                                                        <th>{{ $t('note') }}</th>\n                                                                        <th>{{ $t('topic') }}</th>\n                                                                        <th>{{ $t('who') }}</th>\n                                                                    </tr>\n                                                                    </thead>\n                                                                    <tbody>\n                                                                    <tr>\n                                                                        <td>01</td>\n                                                                        <td>JB00009</td>\n                                                                        <td>15/July/2020</td>\n                                                                        <td>VARIANCE</td>\n                                                                    </tr>\n                                                                    </tbody>\n                                                                </template>\n                                                            </v-simple-table>\n                 