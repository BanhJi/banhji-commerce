2\" max-width=\"500px\">\n                <template v-slot:activator=\"{ on }\">\n                    <v-btn outlined class=\"secondary--text mx-3 capitalize float-right\" color=\"primary\" v-on=\"on\" @click=\"importShow\">\n                        {{ $t('import') }}\n                    </v-btn>\n                </template>\n                <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n                    <v-card>\n                        <v-card-title>{{ $t('import_customer') }}</v-card-title>\n                        <v-icon class=\"btn_close\" @click=\"dialogm2 = false\">close</v-icon>\n                        <v-divider/>\n                        <v-card-text style=\"background-color: #EDF1F5; color: #333333;\">\n                            <v-row>\n                                <v-col sm=\"12\" cols=\"12\" class=\"pb-0\">\n                                    <label class=\"label\">{{ $t('customer_type') }}</label>\n                                    <v-select\n                                        class=\"mt-1\"\n                                        :placeholder=\"$t('select_type')\"\n                                        outlined\n                                        v-model=\"impCusType\"\n                                        :items=\"customerTypes\"\n                                        @change=\"customerTypeChange\"\n                                        item-value=\"value.id\"\n                                        item-text=\"name\"\n                                        return-object\n                                        required\n                                    />\n                                    <label class=\"label\">{{ $t('group') }}</label>\n                                    <v-select\n                                        class=\"mt-1\"\n                                        :placeholder=\"$t('select_group')\"\n                                        outlined\n                                  