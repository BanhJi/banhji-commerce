CA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA","file":"SubGroup.vue","sourceRoot":"src/views/services_projects/setting","sourcesContent":["<template>\n  <v-row>\n    <v-col sm=\"10\" cols=\"12\" class=\"pt-0\">\n      <h2 class=\"mb-0 font_20\">{{ $t(\"service_sub_group\") }}</h2>\n      <p class=\"mb-0\">{{ $t(\"service_sub_group_desc\") }}</p>\n    </v-col>\n    <v-col sm=\"2\" cols=\"12\" class=\"pt-0\">\n      <v-dialog v-model=\"dialogM2\" max-width=\"400px\" eager>\n        <template v-slot:activator=\"{ on }\">\n          <v-btn\n            color=\"primary\"\n            class=\" text-capitalize white--text float-right\"\n            v-on=\"on\"\n            @click=\"onNewClick\"\n          >\n            {{ $t(\"create_new\") }}\n          </v-btn>\n        </template>\n        <v-card>\n          <div class=\"modal_header\">\n            <v-card-title>{{ $t(\"sub_group\") }}</v-card-title>\n            <v-icon class=\"btn_close\" @click=\"dialogM2 = false\">close</v-icon>\n          </div>\n\n          <v-card-text class=\"modal_text_content\">\n            <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n              <v-row>\n                <v-col sm=\"12\" cols=\"12\" class=\"pb-0\">\n                  <label class=\"label\">{{ $t(\"name\") }}</label>\n                  <v-text-field\n                    class=\"mt-1\"\n                    outlined\n                    :rules=\"[\n                      (v) => !!v || $t('name_is_required'),\n                      (v) => (v && v.length > 2) || $t('character_3'),\n                    ]\"\n                    required\n                    placeholder=\"\"\n                    v-model=\"subGroup.name\"\n                  />\n                </v-col>\n                <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                  <v-row>\n                    <v-col sm=\"4\" cols=\"4\" class=\"py-0\">\n                      <label class=\"label\">{{ $t(\"abbreviation\") }}</label>\n                      <v-text-field\n                        class=\"mt-1\"\n                        outlined\n                        tage=\"Abbreviation\"\n                        :rules=\"[(v) => !!v || $t('abbr_is_required')]\"\n                        required\n                        placeholder=\"\"\n                        v-model=\"subGroup.abbr\"\n                      />\n                    </v-col>\n                    <v-col sm=\"8\" cols=\"8\" class=\"py-0\">\n                      <label class=\"label\">{{ $t(\"group\") }}</label>\n                      <v-select\n                        :loading=\"loading\"\n                        class=\"mt-1\"\n                        tag=\"group\"\n                        v-model=\"mGroup\"\n                        :items=\"groups\"\n                        item-value=\"id\"\n                        :item-text=\"(item) => `${item.abbr} - ${item.name}`\"\n                        :rules=\"[(v) => !!v || $t('type_is_required')]\"\n                        required\n                        return-object\n                        outlined\n                      />\n                    </v-col>\n                  </v-row>\n                </v-col>\n                <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                  <label class=\"label\">{{ $t(\"color\") }}</label>\n                  <v-row>\n                    <v-col class=\"py-1\">\n                      <kendo-colorpalette\n                        class=\"mt-1\"\n                        id=\"colorPickSubGroupDS\"\n                        :palette=\"palette\"\n                        :change=\"onGetColor\"\n                        :tile-size=\"40\"\n                      />\n                    </v-col>\n                  </v-row>\n                </v-col>\n                <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                  <h2 class=\"mb-0 font_20 width_100\">\n                    {{ $t(\"default_image\") }}\n                  </h2>\n                  <v-row>\n                    <v-col sm=\"6\" cols=\"12\" lass=\"pt-0\">\n                      <p class=\"color_grey font_14\">\n                        {{ $t(\"add_funct_prod_variant_desc3\") }}\n                      </p>\n                      <v-btn\n                        class=\"float-left  btn_cancel\"\n                        @click=\"handleUpload\"\n                      >\n                        {{ $t(\"upload\") }}\n                      </v-btn>\n                    </v-col>\n                    <v-col sm=\"6\" cols=\"12\" class=\"py-0\">\n                      <v-img width=\"150px\" height=\"150px\" :src=\"src\" />\n                    </v-col>\n                    <v-col sm=\"6\" cols=\"12\">\n                      <label>\n                        <input\n                          hidden\n                          type=\"file\"\n                          name=\"image\"\n                          id=\"image\"\n                          :rules=\"fileSizeRules\"\n                          accept=\".png, .jpg, .jpeg\"\n                          ref=\"fileupload\"\n                          @change=\"onFileChange\"\n                        />\n                      </label>\n                    </v-col>\n                  </v-row>\n                </v-col>\n              </v-row>\n            </v-form>\n          </v-card-text>\n          <v-divider />\n          <v-card-actions class=\"modal_footer\">\n            <v-row>\n              <v-col sm=\"6\" cols=\"6\" class=\"py-0 text-left\">\n                <v-btn\n                  color=\"black\"\n                  outlined\n                  class=\" text-capitalize  black--text float-left\"\n                  @click=\"dialogM2 = false\"\n                  >{{ $t(\"cancel\") }}\n                </v-btn>\n              </v-col>\n              <v-col sm=\"6\" cols=\"6\" class=\"py-0 text-right\">\n                <v-btn\n                  color=\"primary\"\n                  class=\"px-3  white--text text-capitalize\"\n                  @click=\"onSaveClose\"\n                >\n                  {{ $t(\"save_close\") }}\n                </v-btn>\n              </v-col>\n            </v-row>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n    </v-col>\n\n    <v-col sm=\"12\" cols=\"12\">\n      <template>\n        <LoadingMe\n          :isLoading=\"showLoading\"\n          :fullPage=\"false\"\n          :myLoading=\"true\"\n          type=\"loading\"\n        />\n        <kendo-datasource ref=\"gridSubGroupDS\" :data=\"subGroups\" />\n        <kendo-grid\n          id=\"gridSubGroup\"\n          class=\"grid-function\"\n          :data-source-ref=\"'gridSubGroupDS'\"\n          :editable=\"false\"\n          :scrollable-virtual=\"true\"\n        >\n          <kendo-grid-column\n            :field=\"'no'\"\n            :title=\"$t('no')\"\n            :template=\"rowNumberTmpl\"\n            :width=\"40\"\n            :column-menu=\"false\"\n            :headerAttributes=\"{\n              style: 'background-color: #EDF1F5;',\n              class: 'text-center',\n            }\"\n            :attributes=\"{ style: 'text-align: center' }\"\n          />\n          <kendo-grid-column\n            :field=\"'abbr'\"\n            :title=\"$t('abbr')\"\n            :width=\"50\"\n            :template=\"'<span>#=abbr#</span>'\"\n            :headerAttributes=\"{ style: 'background-color: #EDF1F5' }\"\n          />\n          <kendo-grid-column\n            :field=\"'name'\"\n            :attributes=\"{ class: 'tb_name_td' }\"\n            :title=\"$t('name')\"\n            :width=\"90\"\n            :template=\"'<span>#=name#</span>'\"\n            :headerAttributes=\"{ style: 'background-color: #EDF1F5' }\"\n          />\n          <kendo-grid-column\n            :field=\"'group'\"\n            :title=\"$t('group')\"\n            :width=\"100\"\n            :template=\"'<span>#=group.name#</span>'\"\n            :headerAttributes=\"{ style: 'background-color: #EDF1F5' }\"\n          />\n          <kendo-grid-column\n            :field=\"''\"\n            :title=\"$t('action')\"\n            :width=\"70\"\n            :command=\"[\n              { iconClass: 'k-icon k-i-edit', text: 'Edit', click: goEdit },\n            ]\"\n            :headerAttributes=\"{\n              style: 'text-align: left; background-color: #EDF1F5',\n            }\"\n          />\n        </kendo-grid>\n      </template>\n    </v-col>\n  </v-row>\n</template>\n\n<script>\nimport { ColorPalette } from \"@progress/kendo-inputs-vue-wrapper\";\n\nconst groupHandler = require(\"@/scripts/groupHandler\");\nconst subGroupHandler = require(\"@/scripts/subGroupHandler\");\n\nconst $ = require(\"jquery\");\nimport kendo from \"@progress/kendo-ui\";\nexport default {\n  components: {\n    LoadingMe: () => import(`@/components/Loading`),\n    \"kendo-colorpalette\": ColorPalette,\n  },\n  data: () => ({\n    palette: [\n      \"#E6A044\",\n      \"#EBE4E4\",\n      \"#D42E21\",\n      \"#000\",\n      \"#242A34\",\n      \"#9FCC63\",\n      \"#519D36\",\n    ],\n    fileSizeRules: [\n      (files) =>\n        !files ||\n        !files.some((file) => file.size > 1024 * 1024) ||\n        \"Image size should be less than 1 MB!\",\n    ],\n    src: \"\",\n    dialogM2: false,\n    showLoading: true,\n    valid: true,\n    subGroup: {\n      id: \"\",\n      name: \"\",\n      abbr: \"\",\n      type: \"\",\n      img: \"\",\n      group: {},\n    },\n    subGroups: [],\n    groupId: \"\",\n    groups: [],\n    mGroup: \"\",\n    imgFile: \"\",\n    colorPick: \"#242A34\",\n    image: \"\",\n    loading: true,\n  }),\n  props: {},\n  computed: {},\n  watch: {\n    dialogm2(val) {\n      val || this.close();\n    },\n  },\n  created() {},\n  methods: {\n    onGetColor() {\n      let grid = kendo.jQuery(\"#colorPickSubGroupDS\").data(\"kendoColorPalette\");\n      this.colorPick = grid.value();\n      // window.console.log(this.colorPick, grid, 'color')\n    },\n    handleUpload() {\n      this.$refs[\"fileupload\"].click();\n    },\n    onFileChange(e) {\n      // const file = e.target.files[0];\n      let input = e.target;\n      // Ensure that you have a file before attempting to read it\n      if (input.files && input.files[0]) {\n        if (input.files[0].size > 1024 * 1024) {\n          e.preventDefault();\n          alert(\"File too big (> 1MB)\");\n          this.src = \"\";\n          this.imgFile = \"\";\n          this.$refs.fileupload.value = null;\n        } else {\n          // create a new FileReader to read this image and convert to base64 format\n          let reader = new FileReader();\n          // Define a callback function to run, when FileReader finishes its job\n          reader.onloadend = (e) => {\n            // Note: arrow function used here, so that \"this.imageData\" refers to the imageData of Vue component\n            // Read image as base64 and set to imageData\n            this.imgFile = e.target.result;\n          };\n          // Start the reader job - read file as a data url (base64 format)\n          reader.readAsBinaryString(input.files[0]);\n          // this.url = input.files[0]\n          this.src = URL.createObjectURL(e.target.files[0]);\n        }\n      }\n    },\n    rowNumberTmpl(dataItem) {\n      let ds = this.$refs.gridSubGroupDS.kendoWidget(),\n        index = ds.indexOf(dataItem);\n      return index + 1;\n    },\n    goEdit(e) {\n      e.preventDefault();\n      let grid = kendo.jQuery(\"#gridSubGroup\").data(\"kendoGrid\");\n      let dataItem = grid.dataItem($(e.currentTarget).closest(\"tr\"));\n      this.subGroup = dataItem;\n      this.mGroup = {\n        abbr:       dataItem.group.abbr || '',\n        category:   dataItem.group.category || {},\n        color:      dataItem.group.color || '',\n        id:         dataItem.group.id || '',\n        img:        dataItem.group.img || {},\n        name:       dataItem.group.name || ''\n      }\n      window.console.log(12,this.mGroup)\n      this.image = dataItem.img;\n      this.colorPick = dataItem.color;\n      this.subGroup.type = \"edit\";\n      if (dataItem.img.hasOwnProperty(\"default\")) {\n        this.src =\n          \"https://s3-ap-southeast-1.amazonaws.com/images.banhji/\" +\n          dataItem.img.default.bigThumb;\n      } else {\n        this.src = \"\";\n      }\n      this.dialogM2 = true;\n      this.loadGroup()\n    },\n    onNewClick() {\n      this.subGroup.id = \"\";\n      this.subGroup.type = \"\";\n      this.clear();\n      this.loadGroup()\n    },\n    close() {\n      this.clear();\n      this.dialogM2 = false;\n    },\n    clear() {\n      this.subGroup = {};\n      this.src = \"\";\n      this.mGroup = {};\n      this.imgFile = \"\";\n      this.colorPick = \"\";\n      // let grid = $('#colorPickSubGroupDS').data(\"kendoColorPalette\")\n      // grid.value(null)\n    },\n    async onSaveClose() {\n      if (!this.$refs.form.validate()) {\n        this.$refs.form.validate();\n        return;\n      }\n      new Promise((resolve) => {\n        setTimeout(() => {\n          resolve(\"resolved\");\n          let data = {\n            id: this.subGroup.id ? this.subGroup.id : \"\",\n            abbr: this.subGroup.abbr,\n            name: this.subGroup.name,\n            group: this.mGroup,\n            img: this.imgFile,\n            image: this.image,\n            type: this.subGroup.type,\n            color: this.colorPick,\n          };\n          // window.console.log(data)\n          subGroupHandler\n            .create(data)\n            .then((response) => {\n              if (response.data.statusCode === 201) {\n                this.dialogM2 = false;\n                // this.subGroups = []\n                // const res = response.data.data\n                // const index = this.subGroups.findIndex(item => {\n                //     return (res.id === item.id)\n                // })\n                // if (index < 0) {\n                //     if (this.subGroups.length === 0) {\n                //         this.subGroups = [res]\n                //     } else {\n                //         this.subGroups.push(res)\n                //     }\n                //     const grid = kendo.jQuery(\"#gridSubGroup\").data(\"kendoGrid\");\n                //     grid.refresh()\n                //\n                // } else {\n                //     this.subGroups.splice(index, 1, res)\n                //     const grid = kendo.jQuery(\"#gridSubGroup\").data(\"kendoGrid\");\n                //     grid.refresh()\n                // }\n                // if (this.subGroups.length === 0) {\n                //     this.loadSubGroup()\n                // }\n                this.loadSubGroup();\n                this.clear();\n                this.close();\n                this.$refs.form.reset();\n              }\n            })\n            .catch((e) => {\n              this.errors.push(e);\n            });\n        }, 300);\n      });\n    },\n    async loadSubGroup() {\n      new Promise((resolve) => {\n        setTimeout(() => {\n          resolve(\"resolved\");\n          subGroupHandler.get().then((res) => {\n            this.showLoading = false;\n            this.subGroups = res.filter(\n              (o) => o.group.category.type.name === \"Service\"\n            );\n            // res.forEach(i => {\n            //     const group = i.group\n            //     const category = group.category\n            //     if (category.hasOwnProperty('type')) {\n            //         if (group.category.type.name === 'Service') {\n            //             this.subGroups.push(i)\n            //         }\n            //     }\n            // })\n            // window.console.log()\n            // this.subGroups = res\n          });\n        }, 300);\n      });\n    },\n    async loadGroup() {\n      new Promise((resolve) => {\n        setTimeout(() => {\n          resolve(\"resolved\");\n          groupHandler.get().then((res) => {\n            this.showLoading = false;\n            // res.forEach(i => {\n            //     const category = i.category\n            //     if (category.hasOwnProperty('type')) {\n            //         if (i.category.type.name === \"Service\") {\n            //             this.groups.push(i)\n            //         }\n            //     }\n            // })\n            this.groups = res.filter((o) => o.category.type.name === \"Service\");\n            this.loading = false\n          });\n        }, 300);\n      });\n    },\n  },\n  mounted: async function() {\n    await this.loadSubGroup();\n    // await this.loadGroup();\n  },\n};\n</script>\n<style scoped>\n.acc_group header {\n  height: 10px !important;\n}\n\ntable.acc_group tr td {\n  border-bottom: 1px solid #ccc;\n  padding: 8px;\n  vertical-align: middle;\n}\n\ntable.acc_group tr td:first-child {\n  font-weight: 700;\n}\n\ntable.acc_group tr td:last-child {\n  text-align: center;\n}\n\n.v-card__actions .v-btn.v-btn {\n  padding: 0 16px;\n}\n\n.exchange-table.theme--light.v-data-table\n  > .v-data-table__wrapper\n  > table\n  > thead\n  > tr\n  > th {\n  font-family: \"Niradei-Bold\", serif;\n  color: #000 !important;\n  border-top: 1px solid #000 !important;\n  border-bottom: 1px solid #000 !important;\n  font-size: 15px !important;\n}\n\n.exchange-table.theme--light.v-data-table\n  > .v-data-table__wrapper\n  > table\n  > thead\n  > tr\n  > td {\n  color: #000 !important;\n  padding: 5px !important;\n}\n\n.exchange-table.theme--light.v-data-table\n  > .v-data-table__wrapper\n  > table\n  > tbody\n  > tr:last-child\n  td {\n  border-bottom: 1px solid #000 !important;\n}\n\n.function_footer {\n  padding: 15px;\n  display: inline-block;\n}\n\np {\n  color: rgba(0, 0, 0, 0.87);\n}\n\n.actionBtn {\n  height: 47.2px !important;\n}\n\n.text_tip {\n  font-size: 9px;\n  line-height: 10px;\n}\n\n.v-text-field__details {\n  margin-bottom: 8px;\n  margin-top: 4px !important;\n}\n\n@media (max-width: 576px) {\n}\n</style>\n"]}]}