"\n                                    :title=\"$t('type')\"\n                                    :width=\"120\"\n                                    :sortable=\"false\"\n                                    :template=\"'<span>#=transactionType#</span>'\"\n                                    :headerAttributes=\"{ style: 'background-color: #EDF1F5' }\"/>\n                                <kendo-grid-column\n                                    :field=\"'referenceNo'\"\n                                    :title=\"$t('num')\"\n                                    :width=\"150\"\n                                    :sortable=\"false\"\n                                    :template=\"referenceTemplate\"\n                                    :headerAttributes=\"{ style: 'background-color: #EDF1F5, color: green !important' }\"/>\n                                <!--                                <kendo-grid-column-->\n                                <!--                                    :field=\"'referenceNo'\"-->\n                                <!--                                    :title=\"$t('ref_from')\"-->\n                                <!--                                    :width=\"180\"-->\n                                <!--                                    :template=\"referenceTemplate\"-->\n                                <!--                                    :headerAttributes=\"{ style: 'background-color: #EDF1F5, color: green !important' }\"/>-->\n                                <!--                                <kendo-grid-column-->\n                                <!--                                    :field=\"'referenceNo'\"-->\n                                <!--                                    :title=\"$t('ref_to')\"-->\n                                <!--                                    :width=\"180\"-->\n                                <!--                                    :template=\"referenceTemplate\"-->\n                     