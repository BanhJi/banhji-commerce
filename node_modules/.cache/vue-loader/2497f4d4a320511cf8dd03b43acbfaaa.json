itle=\"$t('base_uom')\"\n                            :width=\"160\"\n                            :editable=\"\n                            () => {\n                              return false;\n                            }\n                          \"\n                            :headerAttributes=\"{\n                            style: 'background-color: #EDF1F5',\n                          }\"\n                        />\n                        <kendo-grid-column\n                            :field=\"'qty'\"\n                            :title=\"$t('qty')\"\n                            :width=\"150\"\n                            :template=\"\n                            '<span>#=kendo.toString(qty || 0, decimalFormat) || 0#</span>'\n                          \"\n                            :headerAttributes=\"{\n                            style:\n                              'text-align: right; background-color: #EDF1F5',\n                          }\"\n                            :attributes=\"{\n                            style: 'text-align: right',\n                          }\"\n                        />\n                        <kendo-grid-column\n                            :field=\"'cost'\"\n                            :title=\"$t('cost')\"\n                            :width=\"150\"\n                            :template=\"\n                            '<span>#=kendo.toString(cost || 0, decimalFormat) || 0#</span>'\n                          \"\n                            :headerAttributes=\"{\n                            style:\n                              'text-align: right; background-color: #EDF1F5',\n                          }\"\n                            :attributes=\"{\n                            style: 'text-align: right',\n                          }\"\n                        />\n                        <kendo-grid-column\n                            :field=\"'amount'\"\n                            :title=\"$t('amount')\"\n          