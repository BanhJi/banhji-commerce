t/>\n\t\t\t\t\t</v-col>\n\t\t\t\t</v-row>\n\t\t\t\t<v-row class=\"mt-0\">\n\t\t\t\t\t<v-col sm=\"4\" cols=\"12\" class=\"py-0 pt-6\">\n\t\t\t\t\t\t<app-month-of-picker\n\t\t\t\t\t\t\t:initMonthOf=\"monthOf\"\n\t\t\t\t\t\t\t@emitMonthOf=\"monthOf = $event\"\n\t\t\t\t\t\t\t@onChanged=\"onMonthOfChanges\"\n\t\t\t\t\t\t\t:initMin=\"minMonth\"\n\t\t\t\t\t\t\t:initMax=\"maxMonth\"\n\t\t\t\t\t\t/>\n\t\t\t\t\t</v-col>\n\t\t\t\t\t<v-col sm=\"4\" cols=\"12\" class=\"py-0 pt-6\">\n\t\t\t\t\t\t<v-select\n\t\t\t\t\t\t\tclass=\"mt-1\"\n\t\t\t\t\t\t\t:disabled=\"monthOf === ''\"\n\t\t\t\t\t\t\tplaceholder=\"Select Budget\"\n\t\t\t\t\t\t\tv-model=\"budget\"\n\t\t\t\t\t\t\t:items=\"budgetList\"\n\t\t\t\t\t\t\titem-text=\"name\"\n\t\t\t\t\t\t\titem-value=\"uuid\"\n\t\t\t\t\t\t\treturn-object\n\t\t\t\t\t\t\tclearable\n\t\t\t\t\t\t\toutlined\n\t\t\t\t\t\t/>\n\t\t\t\t\t</v-col>\n\t\t\t\t\t<v-col sm=\"4\" cols=\"12\" class=\"py-0\">\n\t\t\t\t\t\t<v-btn\n\t\t\t\t\t\t\tcolor=\"primary\"\n\t\t\t\t\t\t\tclass=\"text-capitalize mt-7 white--text\"\n\t\t\t\t\t\t\t@click=\"search\"\n\t\t\t\t\t\t>\n\t\t\t\t\t\t\t{{ $t(\"run_report\") }}\n\t\t\t\t\t\t</v-btn>\n\t\t\t\t\t</v-col>\n\t\t\t\t</v-row>\n\n\t\t\t\t<!-- Summary -->\n\t\t\t\t<v-row>\n\t\t\t\t\t<v-col class=\"pt-0\" sm=\"6\" cols=\"12\">\n\t\t\t\t\t\t<template>\n\t\t\t\t\t\t\t<v-simple-table class=\"table_list nomal_table\">\n\t\t\t\t\t\t\t\t<template>\n\t\t\t\t\t\t\t\t\t<tbody>\n\t\t\t\t\t\t\t\t\t\t<tr>\n\t\t\t\t\t\t\t\t\t\t\t<td style=\"width: 45%\" class=\"\">\n\t\t\t\t\t\t\t\t\t\t\t\t{{ $t(\"budget_name\") }}\n\t\t\t\t\t\t\t\t\t\t\t</td>\n\t\t\t\t\t\t\t\t\t\t\t<td class=\"\">{{ budget.name }}</td>\n\t\t\t\t\t\t\t\t\t\t</tr>\n\t\t\t\t\t\t\t\t\t\t<tr>\n\t\t\t\t\t\t\t\t\t\t\t<td class=\"\">{{ $t(\"segment\") }}</td>\n\t\t\t\t\t\t\t\t\t\t\t<td class=\"btb\" v-if=\"budget.segment\">\n\t\t\t\t\t\t\t\t\t\t\t\t{{ budget.segment.name }}\n\t\t\t\t\t\t\t\t\t\t\t</td>\n\t\t\t\t\t\t\t\t\t\t</tr>\n\t\t\t\t\t\t\t\t\t\t<tr>\n\t\t\t\t\t\t\t\t\t\t\t<td class=\"\">{{ $t(\"project\") }}</td>\n\t\t\t\t\t\t\t\t\t\t\t<td class=\"\" v-if=\"budget.project\">\n\t\t\t\t\t\t\t\t\t\t\t\t{{ budget.project.name }}\n\t\t\t\t\t\t\t\t\t\t\t</td>\n\t\t\t\t\t\t\t\t\t\t</tr>\n\t\t\t\t\t\t\t\t\t\t<tr>\n\t\t\t\t\t\t\t\t\t\t\t<td class=\"\">{{ $t(\"fiscal_year\") }}</td>\n\t\t\t\t\t\t\t\t\t\t\t<td class=\"\" v-if=\"budget.fiscal_year\">\n\t\t\t\t\t\t\t\t\t\t\t\t{{\n\t\t\t\t\t\t\t\t\t\t\t\t\tHelper.formatFiscalDate(budget.fiscal_year.start_date)\n\t\t\t\t\t\t\t\t\t\t\t\t}}\n\t\t\t\t\t\t\t\t\t\t\t</td>\n\t\t\t\t\t\t\t\t\t\t</tr>\n\t\t\t\t\t\t\t\t\t</tbody>\n\t\t\t\t\t\t\t\t</template>\n\t\t\t\t\t\t\t</v-simple-table>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</v-col>\n\t\t\t\t\t<v-col class=\"pt-0\" sm=\"6\" cols=\"12\">\n\t\t\t\t\t\t<template>\n\t\t\t\t\t\t\t<v-simple-table class=\"table_list nomal_table\">\n\t\t\t\t\t\t\t\t<template>\n\t\t\t\t\t\t\t\t\t<tbody>\n\t\t\t\t\t\t\t\t\t\t<tr>\n\t\t\t\t\t\t\t\t\t\t\t<td class=\"\">{{ $t(\"created_on\") }}</td>\n\t\t\t\t\t\t\t\t\t\t\t<!-- <td class=\"\">{{ budget.created_date }}</td> by rothny -->\n\t\t\t\t\t\t\t\t\t\t\t<td class=\"\" v-if=\"budget.created_date\">\n\t\t\t\t\t\t\t\t\t\t\t\t{{ Helper.dateFormat(budget.created_date) }}\n\t\t\t\t\t\t\t\t\t\t\t</td>\n\t\t\t\t\t\t\t\t\t\t</tr>\n\t\t\t\t\t\t\t\t\t\t<tr>\n\t\t\t\t\t\t\t\t\t\t\t<td class=\"\">{{ $t(\"last_edited_on\") }}</td>\n\t\t\t\t\t\t\t\t\t\t\t<td class=\"\">{{ budget.modified_date }}</td>\n\t\t\t\t\t\t\t\t\t\t</tr>\n\t\t\t\t\t\t\t\t\t\t<tr>\n\t\t\t\t\t\t\t\t\t\t\t<td class=\"\">{{ $t(\"last_edited_by\") }}</td>\n\t\t\t\t\t\t\t\t\t\t\t<td class=\"\">\n\t\t\t\t\t\t\t\t\t\t\t\t{{\n\t\t\t\t\t\t\t\t\t\t\t\t\tbudget.modified_by.first_name === \"\"\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t? budget.created_by.first_name +\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\" \" +\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tbudget.created_by.last_name\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t: budget.modified_by.first_name +\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\" \" +\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\tbudget.modified_by.last_name\n\t\t\t\t\t\t\t\t\t\t\t\t}}\n\t\t\t\t\t\t\t\t\t\t\t</td>\n\t\t\t\t\t\t\t\t\t\t</tr>\n\t\t\t\t\t\t\t\t\t</tbody>\n\t\t\t\t\t\t\t\t</template>\n\t\t\t\t\t\t\t</v-simple-table>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</v-col>\n\t\t\t\t</v-row>\n\n\t\t\t\t<!-- Grid -->\n\t\t\t\t<v-row>\n\t\t\t\t\t<v-col sm=\"12\" cols=\"12\" class=\"pt-1\">\n\t\t\t\t\t\t<template>\n\t\t\t\t\t\t\t<treelistdatasource \n\t\t\t\t\t\t\t\tref=\"statementDS\"\n\t\t\t\t\t\t\t\t:data=\"statementList\"\n\t\t\t\t\t\t\t\t:schema-model=\"model\"\n\t\t\t\t\t\t\t\t:aggregate=\"aggregateDefinition\">\n\t\t\t\t\t\t\t</treelistdatasource>\n\n\t\t\t\t\t\t\t<treelist \n\t\t\t\t\t\t\t\tref=\"statementGrid\"\n\t\t\t\t\t\t\t\tdata-source-ref=\"statementDS\"\n\t\t\t\t\t\t\t\t:columns=\"columns\" />\n\n\t\t\t\t\t\t\t<LoadingMe\n\t\t\t\t\t\t\t\ttype=\"loading\"\n\t\t\t\t\t\t\t\t:isLoading=\"showLoading\"\n\t\t\t\t\t\t\t\t:myLoading=\"true\"\n\t\t\t\t\t\t\t\t:fullPage=\"false\"\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</v-col>\n\t\t\t\t</v-row>\n\t\t\t</v-card>\n\t\t</v-col>\n\t</v-row>\n</template>\n\n<script>\nimport kendo from \"@progress/kendo-ui\";\nimport { i18n } from \"@/i18n\";\nimport Helper from \"@/helper.js\";\n/* Models */\nimport SegmentModel from \"@/scripts/model/Segment.js\";\nimport ProjectModel from \"@/scripts/model/Project.js\";\nimport BudgetModel from \"@/scripts/model/Budget.js\";\nimport ClassModel from \"@/scripts/model/Clazz.js\";\nimport ClassBudgetPerformanceModel from \"@/scripts/model/ClassBudgetPerformance.js\";\n\nimport { TreeList } from '@progress/kendo-treelist-vue-wrapper';\nimport { TreeListDataSource } from '@progress/kendo-datasource-vue-wrapper';\n\nconst BudgetHandler = require(\"@/scripts/handler/workingcapital/budgetHandler\");\n\nexport default {\n\tcomponents: {\n\t\tLoadingMe: () => import(\"@/components/Loading\"),\n\t\t\"app-fiscal-date-dropdownlist\": () =>\n\t\t\timport(\"@/components/kendo_templates/FiscalDateDropDownList\"),\n\t\t\"app-month-of-picker\": () =>\n\t\t\timport(\"@/components/kendo_templates/MonthOfPicker\"),\n\t\t\"app-segment-dropdownlist\": () =>\n\t\t\timport(\"@/components/dropdownlist/Segment\"),\n\t\t\"app-project-dropdownlist\": () =>\n\t\t\timport(\"@/components/dropdownlist/Project\"),\n\t\t'treelist': TreeList,\n\t\t'treelistdatasource': TreeListDataSource,\n\t},\n\tdata: () => ({\n\t\tmonthOf: \"\",\n\t\tbudget: new BudgetModel(),\n\t\tsegment: new SegmentModel(),\n\t\tproject: new ProjectModel(),\n\t\tbudgetList: [],\n\t\tstatements: [],\n\t\tstatementList: [],\n\t\tfiscalDate: null,\n\t\tfiscalMonths: [],\n\t\tminMonth: \"\",\n\t\tmaxMonth: \"\",\n\t\tHelper: Helper,\n\t\tcolumns: [],\n\t\tsortDefinition: { field: \"code\", dir: \"asc\" },\n\t\tgroupDefinition: [],\n\t\taggregateDefinition: [\n\t\t\t{ field: \"budget_month\", aggregate: \"sum\" },\n\t\t\t{ field: \"budget_ytd\", aggregate: \"sum\" },\n\t\t\t{ field: \"budget_whole\", aggregate: \"sum\" },\n\t\t\t{ field: \"actual_month\", aggregate: \"sum\" },\n\t\t\t{ field: \"actual_ytd\", aggregate: \"sum\" },\n\t\t\t{ field: \"actual_whole\", aggregate: \"sum\" },\n\t\t],\n\t\tmodel: {\n\t\t\tid: \"uuid\",\n\t\t\texpanded: true\n\t\t},\n\t\t// LoadingMe\n\t\tshowLoading: false,\n\t}),\n\tmethods: {\n\t\tonFiscalDateChanges() {\n\t\t\tthis.monthOf = \"\";\n\t\t\tthis.budget = new BudgetModel();\n\t\t\tthis.fiscalMonths = [];\n\n\t\t\tif (this.fiscalDate !== null) {\n\t\t\t\t/* Set Month Range */\n\t\t\t\tthis.minMonth = this.fiscalDate.start_date;\n\t\t\t\tthis.maxMonth = this.fiscalDate.end_date;\n\n\t\t\t\t/* Generate Months */\n\t\t\t\tthis.fiscalMonths = Helper.generateFiscalMonths(\n\t\t\t\t\tthis.fiscalDate.start_date,\n\t\t\t\t\tthis.fiscalDate.end_date,\n\t\t\t\t\t\"12\"\n\t\t\t\t);\n\n\t\t\t\t/* Load Budget */\n\t\t\t\tthis.loadBudget();\n\t\t\t}\n\t\t},\n\t\tonMonthOfChanges() {\n\t\t\tif (this.statements.length > 0) {\n\t\t\t\t/* Bind Data */\n\t\t\t\tthis.bindData();\n\t\t\t}\n\t\t},\n\t\tasync loadBudget() {\n\t\t\tlet budgets = await this.$store.dispatch('budgets/getList'),\n\t\t\t\tsegments = await this.$store.dispatch('segments/getList'),\n\t\t\t\tprojects = await this.$store.dispatch('projects/getList');\n\n\t\t\tlet budgetList = [];\n\t\t\tbudgets.forEach((value) => {\n\t\t\t\tif (value.fiscal_year.uuid === this.fiscalDate.uuid \n\t\t\t\t\t&& value.segment_uuid === this.segment.id \n\t\t\t\t\t&& value.project_uuid === this.project.id) {\n\t\t\t\t\tlet budget = new BudgetModel(value);\n\t\t\t\t\tbudget.segment = new SegmentModel(\n\t\t\t\t\t\tsegments.find((i) => i.id === value.segment_uuid)\n\t\t\t\t\t);\n\t\t\t\t\tbudget.project = new ProjectModel(\n\t\t\t\t\t\tprojects.find((i) => i.id === value.project_uuid)\n\t\t\t\t\t);\n\n\t\t\t\t\tbudgetList.push(budget);\n\t\t\t\t}\n\t\t\t});\n\t\t\tthis.budgetList = budgetList;\n\t\t},\n\t\t/* Search */\n\t\tsearch() {\n\t\t\tif (this.budget.uuid !== \"\") {\n\t\t\t\tlet asOfDate = kendo.toString(new Date(this.monthOf), \"yyyy-MM-dd\") + \"T23:59:59.999Z\";\n\n\t\t\t\t// Empty statementDS\n\t\t\t\tlet ds = this.$refs.statementDS.kendoWidget();\n\t\t\t\tds.data([]);\n\n\t\t\t\t// Query Income Statement\n\t\t\t\tthis.showLoading = true;\n\t\t\t\tBudgetHandler.getActualClassBudget({\n\t\t\t\t\t\tparams: {\n\t\t\t\t\t\t\tas_of_date: asOfDate,\n\t\t\t\t\t\t\tbudget_uuid: this.budget.uuid\n\t\t\t\t\t\t},\n\t\t\t\t\t})\n\t\t\t\t\t.then((res) => {\n\t\t\t\t\t\tif (res.data) {\n\t\t\t\t\t\t\tthis.statements = res.data;\n\t\t\t\t\t\t\tthis.bindData();\n\t\t\t\t\t\t}\n\t\t\t\t\t})\n\t\t\t\t\t.finally(() => {\n\t\t\t\t\t\tthis.showLoading = false;\n\t\t\t\t\t});\n\t\t\t} else {\n\t\t\t\t/* Alert Warning */\n\t\t\t\tthis.$snotify.warning(i18n.t(\"please_select_budget\"));\n\t\t\t}\n\t\t},\n\t\tasync bindData() {\n\t\t\tlet entries = [];\n\n\t\t\t// Classes\n\t\t\tlet classes = await this.$store.dispatch(\"classes/getList\");\n\n\t\t\t/* Group classes */\n\t\t\tlet groupList = {};\n\t\t\tclasses.forEach(value => {\n\t\t\t\tlet clazz = new ClassModel(value);\n\n\t\t\t\t/* Nature */\n\t\t\t\tif(clazz.clazz.id !== \"\"){\n\t\t\t\t\tif(!groupList[clazz.clazz.id]){\n\t\t\t\t\t\tentries.push(new ClassBudgetPerformanceModel({\n\t\t\t\t\t\t\tuuid : clazz.clazz.id,\n\t\t\t\t\t\t\tcode : clazz.clazz.code,\n\t\t\t\t\t\t\tname : clazz.clazz.name,\n\t\t\t\t\t\t\tbudget_month : '',\n\t\t\t\t\t\t\tbudget_ytd : '',\n\t\t\t\t\t\t\tbudget_whole : '',\n\t\t\t\t\t\t\tactual_month : '',\n\t\t\t\t\t\t\tactual_ytd : '',\n\t\t\t\t\t\t\tactual_whole : '',\n\t\t\t\t\t\t\tmonth_percentage : '',\n\t\t\t\t\t\t\tytd_percentage : '',\n\t\t\t\t\t\t\twhole_percentage : '',\n\t\t\t\t\t\t}));\n\t\t\t\t\t\tgroupList[clazz.clazz.id] = value;\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\t/* Type */\n\t\t\t\tif(clazz.classType.id !== \"\"){\n\t\t\t\t\tif(!groupList[clazz.classType.id]){\n\t\t\t\t\t\tentries.push(new ClassBudgetPerformanceModel({\n\t\t\t\t\t\t\tuuid : clazz.classType.id,\n\t\t\t\t\t\t\tcode : clazz.classType.code,\n\t\t\t\t\t\t\tname : clazz.classType.name,\n\t\t\t\t\t\t\tparentId : clazz.clazz.id,\n\t\t\t\t\t\t\tbudget_month : '',\n\t\t\t\t\t\t\tbudget_ytd : '',\n\t\t\t\t\t\t\tbudget_whole : '',\n\t\t\t\t\t\t\tactual_month : '',\n\t\t\t\t\t\t\tactual_ytd : '',\n\t\t\t\t\t\t\tactual_whole : '',\n\t\t\t\t\t\t\tmonth_percentage : '',\n\t\t\t\t\t\t\tytd_percentage : '',\n\t\t\t\t\t\t\twhole_percentage : '',\n\t\t\t\t\t\t}));\n\t\t\t\t\t\tgroupList[clazz.classType.id] = value;\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\t/* Group */\n\t\t\t\tif(clazz.classGroup.id !== \"\"){\n\t\t\t\t\tif(!groupList[clazz.classGroup.id]){\n\t\t\t\t\t\tentries.push(new ClassBudgetPerformanceModel({\n\t\t\t\t\t\t\tuuid : clazz.classGroup.id,\n\t\t\t\t\t\t\tcode : clazz.classGroup.code,\n\t\t\t\t\t\t\tname : clazz.classGroup.name,\n\t\t\t\t\t\t\tparentId : clazz.classType.id,\n\t\t\t\t\t\t\tbudget_month : '',\n\t\t\t\t\t\t\tbudget_ytd : '',\n\t\t\t\t\t\t\tbudget_whole : '',\n\t\t\t\t\t\t\tactual_month : '',\n\t\t\t\t\t\t\tactual_ytd : '',\n\t\t\t\t\t\t\tactual_whole : '',\n\t\t\t\t\t\t\tmonth_percentage : '',\n\t\t\t\t\t\t\tytd_percentage : '',\n\t\t\t\t\t\t\twhole_percentage : '',\n\t\t\t\t\t\t}));\n\t\t\t\t\t\tgroupList[clazz.classGroup.id] = value;\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\t/* Subgroup */\n\t\t\t\tif(clazz.classSubGroup.id !== \"\"){\n\t\t\t\t\tif(!groupList[clazz.classSubGroup.id]){\n\t\t\t\t\t\tentries.push(new ClassBudgetPerformanceModel({\n\t\t\t\t\t\t\tuuid : clazz.classSubGroup.id,\n\t\t\t\t\t\t\tcode : clazz.classSubGroup.code,\n\t\t\t\t\t\t\tname : clazz.classSubGroup.name,\n\t\t\t\t\t\t\tparentId : clazz.classGroup.id,\n\t\t\t\t\t\t\tbudget_month : '',\n\t\t\t\t\t\t\tbudget_ytd : '',\n\t\t\t\t\t\t\tbudget_whole : '',\n\t\t\t\t\t\t\tactual_month : '',\n\t\t\t\t\t\t\tactual_ytd : '',\n\t\t\t\t\t\t\tactual_whole : '',\n\t\t\t\t\t\t\tmonth_percentage : '',\n\t\t\t\t\t\t\tytd_percentage : '',\n\t\t\t\t\t\t\twhole_percentage : '',\n\t\t\t\t\t\t}));\n\t\t\t\t\t\tgroupList[clazz.classSubGroup.id] = value;\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t});\n\t\t\t\n\t\t\t/* Find index of selected month */\n\t\t\t// let monthOfFormated = kendo.toString(new Date(this.monthOf), \"MMM yyyy\");\n\t\t\tlet\tbudgetMonthFormated = kendo.toString(new Date(this.monthOf), \"yyyy-MM\");\n\n\t\t\t// let monthOfIndex = this.fiscalMonths.indexOf(monthOfFormated);\n\n\t\t\t/* Gathering from budget lines */\n\t\t\tthis.budget.lines.forEach((value) => {\n\t\t\t\tlet entry = new ClassBudgetPerformanceModel();\n\n\t\t\t\t/* Set Class Information(Using field name -> account_uuid) */\n\t\t\t\tlet clazz = new ClassModel(\n\t\t\t\t\tclasses.find((i) => i.id === value.account_uuid)\n\t\t\t\t);\n\t\t\t\tentry.uuid = clazz.id;\n\t\t\t\tentry.code = clazz.code;\n\t\t\t\tentry.name = clazz.name;\n\t\t\t\tentry.natureName = clazz.clazz.name;\n\t\t\t\tentry.typeName = clazz.classType.name;\n\t\t\t\tentry.groupName = clazz.classGroup.name;\n\t\t\t\tentry.subgroupName = clazz.classSubGroup.name;\n\n\t\t\t\t/* Set Parent Id */\n\t\t\t\tif(clazz.clazz.id !== \"\"){/* Nature */\n\t\t\t\t\tentry.parentId = clazz.clazz.id;\n\t\t\t\t}\n\t\t\t\tif(clazz.classType.id !== \"\"){/* Type */\n\t\t\t\t\tentry.parentId = clazz.classType.id;\n\t\t\t\t}\n\t\t\t\tif(clazz.classGroup.id !== \"\"){/* Group */\n\t\t\t\t\tentry.parentId = clazz.classGroup.id;\n\t\t\t\t}\n\t\t\t\tif(clazz.classSubGroup.id !== \"\"){/* Subgroup */\n\t\t\t\t\tentry.parentId = clazz.classSubGroup.id;\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\t/* BUDGET */\n\t\t\t\t/* Budget Whole */\n\t\t\t\tentry.budget_whole = kendo.parseFloat(value.amount);\n\n\t\t\t\tvalue.monthly_amount.forEach((mline) => {\n\t\t\t\t\t/* Budget Month */\n\t\t\t\t\tif (mline.month_of === budgetMonthFormated) {\n\t\t\t\t\t\tentry.budget_month = kendo.parseFloat(mline.amount);\n\t\t\t\t\t}\n\n\t\t\t\t\t/* Budget YTD */\n\t\t\t\t\tif (kendo.toString(new Date(mline.month_of), \"yyyy-MM-dd\") \n\t\t\t\t\t\t<= kendo.toString(new Date(this.monthOf), \"yyyy-MM-dd\")) {\n\t\t\t\t\t\tentry.budget_ytd += kendo.parseFloat(mline.amount);\n\t\t\t\t\t}\n\t\t\t\t});\n\n\t\t\t\t/* ACTUAL */\n\t\t\t\tthis.statements.lines.forEach((plline) => {\n\t\t\t\t\tif (plline.account_uuid === value.account_uuid) {\n\t\t\t\t\t\t/* Actual Whole */\n\t\t\t\t\t\tentry.actual_whole = kendo.parseFloat(plline.actual_amount);/* B Kethya uses this field for actual whole */\n\t\t\t\t\t\t\n\t\t\t\t\t\tplline.actual_monthly_amount.forEach(actMonth => {\n\t\t\t\t\t\t\tlet monthlyAmount = kendo.parseFloat(actMonth.amount);\n\n\t\t\t\t\t\t\t/* Actual Month */\n\t\t\t\t\t\t\tif(actMonth.month_of === this.monthOf){\n\t\t\t\t\t\t\t\tentry.actual_month = monthlyAmount;\n\t\t\t\t\t\t\t}\n\n\t\t\t\t\t\t\t/* Actual YTD */\n\t\t\t\t\t\t\tif(new Date(actMonth.month_of) <= new Date(this.monthOf)){\n\t\t\t\t\t\t\t\tentry.actual_ytd += monthlyAmount;\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t});\n\t\t\t\t\t}\n\t\t\t\t});\n\n\t\t\t\t/* Percentage = Actual / Budget */\n\t\t\t\tif (entry.budget_month !== 0) {\n\t\t\t\t\tentry.month_percentag