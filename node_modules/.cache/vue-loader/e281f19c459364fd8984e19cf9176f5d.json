ss=\"\"\n                      outlined\n                      @change=\"UpdateFormChange\"\n                      placeholder=\"khmer language\"\n                      v-model=\"invoice_setting.signature.recorded_by.text_kh\"\n                    />\n                  </v-col>\n                </v-row>\n                <v-row class=\"mx-0\">\n                  <!-- color -->\n                  <v-divider />\n\n                  <v-col sm=\"12\" cols=\"4\" class=\"pt-2 pl-0\">\n                    <label>{{ $t(\"color_background\") }}</label>\n                    <div class=\"d-flex\">\n                      <input\n                        class=\"color_option\"\n                        type=\"color\"\n                        @change=\"UpdateFormChange\"\n                        v-model=\"invoice_setting.color.color\"\n                      />\n                      <input\n                        class=\"color_option\"\n                        type=\"color\"\n                        @change=\"UpdateFormChange\"\n                        v-model=\"invoice_setting.color.background\"\n                      />\n                    </div>\n                  </v-col>\n                  <label>{{ $t(\"page_size\") }}</label>\n                  <v-col sm=\"12\" cols=\"4\" class=\"\">\n                    <div>\n                      <v-btn\n                        :class=\"{\n                          'primary--text': invoice_setting.page == 'A4',\n                        }\"\n                        @click=\"invoice_setting.page = 'A4'\"\n                        >A4</v-btn\n                      >\n                      <v-btn\n                        :class=\"{\n                          'primary--text': invoice_setting.page == 'A5',\n                        }\"\n                        @click=\"invoice_setting.page = 'A5'\"\n                        class=\"mx-2\"\n                        >A5</v-btn\n                      >\n                    </div>\n                  </v-col>\n                  <v-col sm=\"12\" cols=\"4\" class=\"pl-0\">\n                    <p>{{ $t(\"show_table_column\") }}</p>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        class=\"mt-0 pt-0\"\n                        v-model=\"invoice_setting.column.one\"\n                      />\n                      <label>{{ $t(\"no.\") }}</label>\n                    </div>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        class=\"mt-0 pt-0\"\n                        v-model=\"invoice_setting.column.two\"\n                      />\n                      <label>{{ $t(\"purchase_no\") }}</label>\n                    </div>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        class=\"mt-0 pt-0\"\n                        v-model=\"invoice_setting.column.three\"\n                      />\n                      <label>{{ $t(\"vendor_invoice_no\") }}</label>\n                    </div>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        class=\"mt-0 pt-0\"\n                        v-model=\"invoice_setting.column.four\"\n                      />\n                      <label>{{ $t(\"due_date\") }}</label>\n                    </div>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        class=\"mt-0\"\n                        v-model=\"invoice_setting.column.five\"\n                      />\n                      <label>{{ $t(\"over_due\") }}</label>\n                    </div>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        class=\"mt-0 pt-0\"\n                        v-model=\"invoice_setting.column.six\"\n                      />\n                      <label>{{ $t(\"payment_term\") }}</label>\n                    </div>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        class=\"mt-0 pt-0\"\n                        v-model=\"invoice_setting.column.seven\"\n                      />\n                      <label>{{ $t(\"payment_option\") }}</label>\n                    </div>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        class=\"mt-0 pt-0\"\n                        v-model=\"invoice_setting.column.eight\"\n                      />\n                      <label>{{ $t(\"ref_no\") }}</label>\n                    </div>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        class=\"mt-0 pt-0\"\n                        v-model=\"invoice_setting.column.nine\"\n                      />\n                      <label>{{ $t(\"status\") }}</label>\n                    </div>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        class=\"mt-0 pt-0\"\n                        v-model=\"invoice_setting.column.ten\"\n                      />\n                      <label>{{ $t(\"currency\") }}</label>\n                    </div>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        class=\"mt-0 pt-0\"\n                        v-model=\"invoice_setting.column.ten1\"\n                      />\n                      <label>{{ $t(\"amount\") }}</label>\n                    </div>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        class=\"mt-0 pt-0\"\n                        v-model=\"invoice_setting.column.ten2\"\n                      />\n                      <label>{{ $t(\"discount\") }}</label>\n                    </div>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        class=\"mt-0 pt-0\"\n                        v-model=\"invoice_setting.column.ten3\"\n                      />\n                      <label>{{ $t(\"amount_to_be_paid\") }}</label>\n                    </div>\n                    <div class=\"d-flex\">\n                      <v-checkbox\n                        @change=\"UpdateFormChange\"\n                        disabled\n                        class=\"mt-0 pt-0\"\n                        v-model=\"invoice_setting.column.ten4\"\n                      />\n                      <label>{{ $t(\"paid_amount\") }}</label>\n                    </div>\n                  </v-col>\n                </v-row>\n              </v-col>\n              <v-col sm=\"9\" cols=\"11\" class=\"bg_white\">\n                <div class=\"ma-3 mt-6\">\n                  <div\n                    :style=\"{\n                      'padding-left':\n                        invoice_setting.margin / 100 + 'in !important',\n                      'padding-right':\n                        invoice_setting.margin / 100 + 'in !important',\n                    }\"\n                    style=\"\n                      box-shadow: rgb(0 0 0 / 20%) 0px 6px 12px 0px;\n                      padding: 72px;\n                      min-height: 297mm;\n                    \"\n                  >\n                    <div v-html=\"form_invoice\"></div>\n                  </div>\n                </div>\n              </v-col>\n            </v-row>\n          </v-card-text>\n          <v-divider />\n          <v-card-actions class=\"modal_footer\">\n            <v-btn\n              @click=\"saveForm\"\n              :loading=\"loading\"\n              class=\"btn_save_close float-right\"\n              >{{ $t(\"save_close\") }}</v-btn\n            >\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n    </v-col>\n  </v-row>\n</template>\n\n<script>\n// const axios = require(\"axios\");\nimport {\n  PaymentVoucher,\n  PaymentVoucher2,\n  print,\n} from \"@/form/paymentVoucher.js\";\nconst {\n  getFormSetting,\n  formSettingCreate,\n  deleteFormSetting,\n} = require(\"@/scripts/settingsHandler.js\");\nimport { i18n } from \"@/i18n\";\n\nlet def = {\n  title: \"Payment Voucher\",\n  logo: { width: 140, height: 140, position: \"left\", hide: false },\n  header: false,\n  form_number: 1,\n  table_header: 1,\n  margin: 75,\n  page: \"A4\",\n  color: { background: \"#3B393E\", color: \"#ffffff\" },\n  column: {\n    one: false,\n    two: false,\n    three: false,\n    four: false,\n    five: true,\n    six: false,\n    seven: true,\n    eight: false,\n    nine: false,\n    ten: false,\n    ten1: true,\n    ten2: true,\n    ten3: false,\n    ten4: true,\n    ten5: false,\n    ten6: false,\n    ten7: false,\n    ten8: false,\n  },\n  signature: {\n    prepared_by: { status: true, text: \"Prepare By\", text_kh: \"រៀបចំដោយ\" },\n    reviewed_by: {\n      status: true,\n      text: \"Reviewed By\",\n      text_kh: \"ត្រួតពិនិត្យដោយ\",\n    },\n    verified_by: {\n      status: true,\n      text: \"Verified By\",\n      text_kh: \"ផ្ទៀងផ្ទាត់​ដោយ\",\n    },\n    approved_by: { status: true, text: \"Approved By\", text_kh: \"អនុម័តដោយ\" },\n    posted_by: { status: true, text: \"Posted By\", text_kh: \"ប្រកាសដោយ\" },\n    recorded_by: { status: true, text: \"Received By\", text_kh: \"ទទួលដោយ\" },\n    option: 1,\n    positon: true,\n  },\n  language: \"English\",\n  address: { title: \"St, 120 Kamport\", align: \"center\" },\n};\nexport default {\n  components: {\n    // LoadingMe\n  },\n  data() {\n    return {\n      pk: \"\",\n      sk: \"\",\n      loading: false,\n      invoice_setting: def,\n      form_invoice: PaymentVoucher(this.items, def),\n      slider_widht: 5,\n      slider_height: 5,\n      image_size: {\n        width: \"100px\",\n        height: \"100px\",\n      },\n      language: [\"English\", \"Khmer\", \"Both\"],\n      address_location: [\"center\", \"left\", \"right\"],\n      dialogm1: \"\",\n      dialog: false,\n    };\n  },\n\n  methods: {\n    ResetForm() {\n      let params = \"?id=\" + this.pk;\n      if (this.pk != \"\") {\n        deleteFormSetting(params).then((res) => {\n          window.console.log(res.data);\n          if (res.data.statusCode === 200) {\n            this.invoice_setting = def;\n            this.$snotify.success(i18n.t(\"success\"));\n            if (this.invoice_setting.form_number === 1) {\n              this.form_invoice = PaymentVoucher(\n                this.items,\n                this.invoice_setting\n              );\n            } else {\n              this.form_invoice = PaymentVoucher2(\n                this.items,\n                this.invoice_setting\n              );\n            }\n          } else {\n            this.$snotify.error(i18n.t(\"failed\"));\n          }\n        });\n      }\n    },\n    saveForm() {\n      this.loading = true;\n      let data = {\n        type: \"form\",\n        formType: \"Payment Voucher\",\n        settings: this.invoice_setting,\n        sk: this.sk,\n        pk: this.pk,\n      };\n      formSettingCreate(data).then((res) => {\n        if (res.data.statusCode == 201) {\n          this.dialog = false;\n          this.loading = false;\n          this.$snotify.success(i18n.t(\"success\"));\n          this.sk = res.data.data.sk;\n          this.pk = res.data.data.pk;\n        } else {\n          this.$snotify.error(i18n.t(\"failed\"));\n        }\n        this.loading = false;\n      });\n    },\n    getFormSettingS() {\n      let params = \"?formType=Payment Voucher\";\n      getFormSetting(params).then((res) => {\n        if (res.data.statusCode === 200) {\n          if (res.data.data.length > 0) {\n            this.sk = res.data.data[\"0\"].sk;\n            this.pk = res.data.data[\"0\"].pk;\n            this.invoice_setting = res.data.data[\"0\"].settings;\n            if (this.invoice_setting.form_number === 1) {\n              this.form_invoice = PaymentVoucher(\n                this.items,\n                this.invoice_setting\n              );\n            } else {\n              this.form_invoice = PaymentVoucher2(\n                this.items,\n                this.invoice_setting\n              );\n            }\n          }\n        }\n      });\n    },\n\n    UpdateFormChange() {\n      if (this.invoice_setting.form_number === 1) {\n        this.form_invoice = PaymentVoucher(this.items, this.invoice_setting);\n      } else {\n        this.form_invoice = PaymentVoucher2(this.items, this.invoice_setting);\n      }\n    },\n    logoPosition(pos) {\n      this.invoice_setting.logo.position = pos;\n      if (this.invoice_setting.form_number === 1) {\n        this.form_invoice = PaymentVoucher(this.items, this.invoice_setting);\n      } else {\n        this.form_invoice = PaymentVoucher2(this.items, this.invoice_setting);\n      }\n    },\n    selectedForm(val) {\n      this.selected_form = val;\n    },\n    _print(id) {\n      print(this.items, id, this.invoice_setting);\n    },\n    EdtiForm() {\n      if (this.invoice_setting.form_number === 1) {\n        this.form_invoice = PaymentVoucher(this.items, this.invoice_setting);\n      } else {\n        this.form_invoice = PaymentVoucher2(this.items, this.invoice_setting);\n      }\n      this.dialog = true;\n    },\n  },\n  computed: {},\n  mounted: async function () {\n    this.getFormSettingS();\n  },\n};\n</scr