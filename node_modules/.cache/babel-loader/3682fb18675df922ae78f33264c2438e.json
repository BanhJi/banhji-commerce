,KANA,CAMA,UAAA,KAAA,EAAA;AACA,sBAAA,IAAA,CAAA,UAAA,CAAA,KAAA;AACA,qBARA;AASA,mBAlBA,EAkBA,GAlBA,CAAA;AAmBA,iBApBA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA0BA;AAvEA,GAvBA;AAgGA,EAAA,OAAA;AAAA,2EAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,KAAA,OAAA,EADA;;AAAA;AAAA;AAAA,qBAEA,KAAA,eAAA,EAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAhGA,CAAA","sourcesContent":["<template>\n    <v-row>\n        <v-col sm=\"12\" cols=\"12\" class=\"grayBg px-6\">\n            <v-card color=\"white\" class=\"pa-3 no_border\" elevation=\"0\">\n                <v-row>\n                    <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                        <h2 class=\"mb-0 font_20\">{{ $t('rack') }}</h2>\n                        <v-dialog v-model=\"dialogm2\" max-width=\"400px\">\n                            <template v-slot:activator=\"{ on }\">\n                                <v-btn color=\"primary\" class=\" text-capitalize white--text float-right\"\n                                       v-on=\"on\"\n                                       @click=\"onNewClick\">\n                                    {{ $t('create_new') }}\n                                </v-btn>\n                            </template>\n                            <v-card>\n                                <div class=\"modal_header\">\n                                    <v-card-title>{{ $t('rack') }}</v-card-title>\n                                    <v-icon @click=\"dialogm2 = false\">close</v-icon>\n                                </div>\n                                <v-card-text class=\"modal_text_content\">\n                                    <v-form\n                                        ref=\"form\"\n                                        v-model=\"valid\"\n                                        lazy-validation>\n                                        <v-row>\n                                            <v-col sm=\"12\" cols=\"12\" class=\"pb-0\">\n                                                <label class=\"label\">{{ $t('name') }}</label>\n                                                <v-text-field class=\"mt-1\"\n                                                              outlined\n                                                              :rules=\"[v => !!v || $t('name_is_required'),v => (v && v.length > 2) || $t('character_3')]\"\n                                                              required\n                                                              placeholder=\"\" v-model=\"rack.name\"/>\n                                                <label class=\"label\">{{ $t('zone') }}</label>\n                                                <v-select class=\" mt-1\"\n                                                          id=\"zon_selector\"\n                                                          :items=\"zoneList\"\n                                                          v-model=\"rack.zone\"\n                                                          @change=\"loadSectionSetting\"\n                                                          item-text=\"name\"\n                                                          tage=\"Zone\"\n                                                          item-value=\"id\"\n                                                          outlined\n                                                          return-object/>\n                                            </v-col>\n                                            <v-col sm=\"12\" cols=\"12\" class=\"pt-0\">\n                                                <v-row>\n                                                    <v-col sm=\"4\" cols=\"4\" class=\"pt-0\">\n                                                        <label class=\"label\">{{ $t('code') }}</label>\n                                                        <v-text-field class=\"mt-1\"\n                                                                      outlined\n                                                                      :rules=\"[v => !!v || $t('code')]\"\n                                                                      required\n                                                                      placeholder=\"\"\n                                                                      v-model=\"rack.code\"/>\n                                                    </v-col>\n                                                    <v-col sm=\"8\" cols=\"8\" class=\"pt-0\">\n                                                        <label class=\"label\">{{ $t('section') }}</label>\n                                                        <v-select class=\" mt-1\"\n                                                                  id=\"sec_selector\"\n                                                                  :items=\"sectionList\"\n                                                                  v-model=\"rack.section\"\n                                                                  item-text=\"name\"\n                                                                  tage=\"Section\"\n                                                                  item-value=\"id\"\n                                                                  outlined\n                                                                  return-object/>\n                                                    </v-col>\n                                                </v-row>\n\n                                            </v-col>\n                                        </v-row>\n                                    </v-form>\n                                </v-card-text>\n                                <v-divider/>\n                                <v-card-actions class=\"modal_footer\">\n                                    <v-row>\n                                        <v-col sm=\"6\" cols=\"6\" class=\"py-0 text-left\">\n                                            <v-btn color=\"primary\" outlined\n                                                   class=\" text-capitalize  black--text float-left\"\n                                                   @click=\"dialogm2 = false\">{{ $t('cancel') }}\n                                            </v-btn>\n                                        </v-col>\n                                        <v-col sm=\"6\" cols=\"6\" class=\"py-0 text-right\">\n                                            <v-btn color=\"primary\" class=\"px-3  white--text text-capitalize\"\n                                                   @click=\"onSaveClose\">\n                                                {{ $t('save_close') }}\n                                            </v-btn>\n                                        </v-col>\n                                    </v-row>\n                                </v-card-actions>\n\n                            </v-card>\n                        </v-dialog>\n                        <p class=\"mb-0\">{{ $t('rack_desc') }} </p>\n                    </v-col>\n                </v-row>\n                <v-row>\n                    <v-col sm=\"12\" cols=\"12\">\n                        <template>\n                            <v-simple-table class=\"exchange-table\">\n                                <template v-slot:default>\n                                    <thead>\n                                    <tr>\n                                        <th class=\"text-uppercase\">{{ $t('code') }}</th>\n                                        <th class=\"text-uppercase\">{{ $t('name') }}</th>\n                                        <th class=\"text-uppercase\">{{ $t('zone') }}</th>\n                                        <th class=\"text-uppercase\">{{ $t('section') }}</th>\n                                        <th class=\"text-uppercase\">{{ $t('action') }}</th>\n                                    </tr>\n                                    </thead>\n                                    <tbody>\n                                    <tr v-for=\"rack in racks\" v-bind:key=\"rack.id\">\n                                        <td>{{ rack.code }}</td>\n                                        <td class=\"text-bold\">{{ rack.name }}</td>\n                                        <td>{{ rack.zone ? rack.zone.name : '' }}</td>\n                                        <td>{{ rack.sec ? rack.sec.name : '' }}</td>\n                                        <td>\n                                            <v-btn class=\"btn_edit_setting\" @click=\"onEditClick(rack)\">\n                                                <v-icon class=\"white--text\" size=\"14\">mdi-pen</v-icon>\n                                                <span\n                                                    class=\"capitalize ml-1 white--text font_14\">{{ $t('edit') }}</span>\n                                            </v-btn>\n                                        </td>\n                                    </tr>\n                                    <LoadingMe\n                                        :isLoading=\"compeletLoading\"\n                                        :fullPage=\"false\"\n                                        :myLoading=\"true\"/>\n                                    </tbody>\n                                </template>\n                            </v-simple-table>\n                        </template>\n                    </v-col>\n                </v-row>\n            </v-card>\n        </v-col>\n    </v-row>\n</template>\n\n<script>\nconst warehouseHandler = require(\"@/scripts/warehouseHandler\")\nexport default {\n    components: {\n        LoadingMe: () => import(`@/components/Loading`)\n    },\n    data: () => ({\n        dialogm2: false,\n        compeletLoading: false,\n        valid: true,\n\n        sectionList: [],\n        zoneList: [],\n        rack: {},\n        racks: []\n    }),\n    props: {},\n    computed: {},\n    watch: {\n        dialogm2(val) {\n            val || this.close()\n        },\n    },\n    created() {\n    },\n    methods: {\n        async onEditClick(value) {\n            new Promise(resolve => {\n                setTimeout(() => {\n                    resolve('resolved');\n                    this.rack = value\n                    this.dialogm2 = true\n                }, 300);\n            })\n        },\n        onNewClick() {\n            this.clear()\n        },\n        close() {\n            this.clear()\n            this.dialogm2 = false\n        },\n        clear() {\n            this.rack = {}\n        },\n        async loadRackSetting() {\n            this.compeletLoading = true\n            warehouseHandler.getRackSettingAll().then(res => {\n                if (res) {\n                    this.racks = res\n                    this.compeletLoading = false\n                }\n            })\n        },\n        async loadZon() {\n            this.compeletLoading = true\n            warehouseHandler.getZonAll().then(res => {\n                if (res) {\n                    this.zoneList = res\n                    this.compeletLoading = false\n                }\n            })\n        },\n        async loadSectionSetting() {\n            warehouseHandler.getSectionSettingAll().then(res => {\n                if (res) {\n                    this.sectionList = res\n                }\n            })\n        },\n        async onSaveClose() {\n            if (!this.$refs.form.validate()) {\n                this.$refs.form.validate()\n                return\n            }\n            new Promise(resolve => {\n                setTimeout(() => {\n                    resolve('resolved');\n                    let data = {\n                        'id': this.rack.id || '',\n                        'name': this.rack.name,\n                        'code': this.rack.code,\n                        'sec': this.rack.section,\n                        'zone': this.rack.zone\n                    }\n                    warehouseHandler.saveRackSetting(data).then(res => {\n                        if (res.data.statusCode === 201) {\n                            this.loadRackSetting()\n                            this.close()\n                            this.$refs.form.reset()\n                        }\n                    }).catch(error => {\n                        self.alertError(error)\n                    })\n                }, 300);\n            })\n        },\n    },\n    mounted: async function () {\n        await this.loadZon()\n        await this.loadRackSetting()\n    }\n};\n</script>\n<style scoped>\n.acc_group header {\n    height: 10px !important;\n}\n\ntable.acc_group tr td {\n    border-bottom: 1px solid #ccc;\n    padding: 8px;\n    vertical-align: middle;\n}\n\ntable.acc_group tr td:first-child {\n    font-weight: 700;\n}\n\ntable.acc_group tr td:last-child {\n    text-align: center;\n}\n\n.v-card__actions .v-btn.v-btn {\n    padding: 0 16px;\n}\n\n.exchange-table.theme--light.v-data-table > .v-data-table__wrapper > table > thead > tr > th {\n    font-family: 'Niradei-Bold', serif;\n    color: #000 !important;\n    border-top: 1px solid #000 !important;\n    border-bottom: 1px solid #000 !important;\n    font-size: 15px !important;\n}\n\n.exchange-table.theme--light.v-data-table > .v-data-table__wrapper > table > thead > tr > td {\n    color: #000 !important;\n    padding: 5px !important;\n}\n\n.exchange-table.theme--light.v-data-table > .v-data-table__wrapper > table > tbody > tr:last-child td {\n    border-bottom: 1px solid #000 !important;\n}\n\n.function_footer {\n    padding: 15px;\n    display: inline-block;\n}\n\np {\n    color: rgba(0, 0, 0, 0.87);\n}\n\n.actionBtn {\n    height: 47.2px !important;\n}\n\n.text_tip {\n    font-size: 9px;\n    line-height: 10px;\n}\n\n.v-text-field__details {\n    margin-bottom: 8px;\n    margin-top: 4px !important;\n}\n\n@media (max-width: 576px) {\n\n}\n</style>"],"sourceRoot":"src/views/products/setting/bin_location"}]}