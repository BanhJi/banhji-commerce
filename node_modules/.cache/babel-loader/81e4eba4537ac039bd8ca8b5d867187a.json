A,CAAA,UAAA,IAAA,EAAA;AACA,iCAAA,GAAA,CAAA,IAAA,CAAA,EAAA,KAAA,IAAA,CAAA,EAAA;AACA,yBAFA,CAAA;;AAGA,4BAAA,MAAA,CAAA,QAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,0BAAA,MAAA,CAAA,QAAA;AACA,yBAFA,MAEA;AACA,8BAAA,KAAA,GAAA,CAAA,EAAA;AACA,4BAAA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA;AACA,2BAFA,MAEA;AACA,4BAAA,MAAA,CAAA,QAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA,EAAA,GAAA,CAAA,IAAA;AACA;AACA;;AACA,wBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,SAAA;;AACA,wBAAA,MAAA,CAAA,KAAA;;AACA,wBAAA,MAAA,CAAA,KAAA;AACA;AACA,qBAnBA,EAmBA,KAnBA,CAmBA,UAAA,CAAA,EAAA;AACA,sBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,sBAAA;;AACA,sBAAA,MAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AACA,qBAtBA;AAuBA,mBA9BA,EA8BA,GA9BA,CAAA;AA+BA,iBAhCA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsCA,KAxDA;AAyDA,IAAA,QAzDA,sBAyDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,oBAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,kBAAA,UAAA,CAAA,YAAA;AACA,oBAAA,OAAA,CAAA,UAAA,CAAA;AACA,oBAAA,cAAA,CAAA,OAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,sBAAA,MAAA,CAAA,QAAA,GAAA,GAAA;AACA,qBAFA;AAGA,mBALA,EAKA,GALA,CAAA;AAMA,iBAPA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAlEA;AAmEA,IAAA,KAnEA,mBAmEA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,KArEA;AAsEA,IAAA,KAtEA,mBAsEA;AACA,WAAA,kBAAA,GAAA,EAAA;AACA,WAAA,IAAA,GAAA,EAAA;AACA;AAzEA,GAvCA;AAkHA,EAAA,OAAA;AAAA,2EAAA;AAAA;AAAA;AAAA;AAAA;AACA,mBAAA,QAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAlHA,CAAA","sourcesContent":["<template>\n    <v-row class=\"\">\n        <v-col sm=\"12\" cols=\"12\" class=\"pt-0\">\n            <h2 class=\"mb-0 font_20\">{{$t('national_social_security_fund')}}</h2>\n            <v-dialog v-model=\"dialogm2\" max-width=\"400px\">\n                <template v-slot:activator=\"{ on }\">\n                    <v-btn color=\"primary\" class=\" text-capitalize white--text float-right\" v-on=\"on\" @click=\"onNewClick\">\n                        {{$t('create_new')}}\n                    </v-btn>\n                </template>\n                <v-card>\n                    <div class=\"modal_header\">\n                    <v-card-title>{{$t('national_social_security_fund')}}</v-card-title>\n                    <v-icon  @click=\"dialogm2 = false\">close</v-icon>\n                    </div>\n                    <v-card-text class=\"modal_text_content\">\n                        <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n                            <v-row>\n                                <v-col sm=\"12\" cols=\"12\" class=\"pb-0\">\n                                    <label class=\"label\">{{$t('name')}}</label>\n                                    <v-text-field class=\"mt-1\"\n                                                outlined\n                                                tage=\"Name\"\n                                                v-model=\"nssf.name\"\n                                                :rules=\"[v => !!v || $t('name_is_required')]\"\n                                                placeholder=\"\"/>\n\n                                    <label class=\"label\">{{$t('number')}}</label>\n                                    <v-text-field class=\"mt-1\"\n                                                outlined\n                                                tage=\"Number\"\n                                                v-model=\"nssf.number\"\n                                                :rules=\"[v => !!v || $t('number_is_required')]\"\n                                                placeholder=\"\"/>\n                                </v-col>\n                            </v-row>\n                        </v-form>\n                    </v-card-text>\n                    <v-card-actions class=\"modal_footer\">\n                        <v-row>\n                            <v-col sm=\"6\" cols=\"6\" class=\"py-0 text-left\">\n                                <v-btn small color=\"black\" outlined class=\" text-capitalize  black--text float-left\" @click=\"dialogm2 = false\">{{$t('cancel')}}</v-btn>\n                            </v-col>\n                            <v-col  sm=\"6\" cols=\"6\" class=\"py-0 text-right\">\n                                <v-btn small color=\"primary\" class=\"px-3  white--text text-capitalize\" @click=\"onSaveClose\">{{$t('save_close')}}\n                                </v-btn>\n                            </v-col>\n                        </v-row>\n                    </v-card-actions>\n\n                </v-card>\n            </v-dialog>\n            <p class=\"mb-0\">{{$t('department_desc')}} </p>\n        </v-col>\n\n        <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n            <template>\n                <kendo-datasource ref=\"NSSFDS\" :data=\"nssfList\" />\n                <kendo-grid\n                    id=\"gridNSSF\"\n                    class=\"grid-function\"\n                    :data-source-ref=\"'NSSFDS'\"\n                    :editable=\"false\"\n                    :scrollable-virtual=\"true\"\n                    :column-menu=\"false\">\n                <kendo-grid-column\n                    :field=\"'name'\"\n                    :title=\"$t('name')\"\n                    :template=\"'<span>#=name#</span>'\"\n                    :headerAttributes=\"{ style: 'background-color: #EDF1F5' }\"/>\n                <kendo-grid-column\n                    :field=\"'number'\"\n                    :title=\"$t('number')\"\n                    :template=\"'<span>#= number#</span>'\"\n                    :headerAttributes=\"{ style: 'background-color: #EDF1F5' }\"/>\n                <kendo-grid-column\n                    :field=\"''\"\n                    :title=\"$t('action')\"\n                    :width=\"100\"\n                    :command=\"[{iconClass: 'k-icon k-i-edit', text: 'Edit',click: goEdit}]\"\n                    :headerAttributes=\"{style: 'text-align: right; background-color: #EDF1F5'}\"/>\n                </kendo-grid>\n            </template>\n        </v-col>\n    </v-row>\n</template>\n\n<script>\n    import NSSF from '@/scripts/model/NSSF'\n    import {i18n} from '@/i18n'\n    import kendo from \"@progress/kendo-ui\";\n    const payrollHandler = require(\"../../../scripts/payrollHandler\")\n    const nssfModel = new NSSF({})\n    const $ = kendo.jQuery;\n\n    export default {\n        components: {},\n        data: () => ({\n            dialogm1: '',\n            dialogm2: false,\n            valid: true,\n            compeletLoading: true,\n            isFullpage: false,\n            snackbar: false,\n            textStatus: 'Success',\n            color: '#3DA72E',\n            timeout: 6000,\n            x: 'right',\n            y: 'top',\n            nssf: nssfModel,\n            nssfList: [],\n            headers: [\n                {\n                    text: i18n.t('name'),\n                    align: 'start',\n                    sortable: false,\n                    value: 'name',\n                },\n                {text: i18n.t('description'), value: 'description', sortable: false,},\n                {text: i18n.t('account_type'), value: 'account_type', sortable: false, align: ' d-none'},\n                {text: i18n.t('account_type'), value: 'account_type.name', sortable: false,},\n                {text: i18n.t('action'), value: 'actions', sortable: false},\n            ],\n            option: 1\n        }),\n        props: {},\n        computed: {},\n        watch: {\n            dialogm2(val) {\n                val || this.close()\n            },\n        },\n        created() {\n        },\n        methods: {\n            goEdit(e){\n                new Promise(resolve => {\n                    setTimeout(() => {\n                    resolve('resolved');\n                    e.preventDefault();\n                    let grid = kendo.jQuery(\"#gridNSSF\").data(\"kendoGrid\");\n                    let dataItem = grid.dataItem($(e.currentTarget).closest(\"tr\"));\n                    window.console.log(dataItem)\n                    this.nssf = dataItem\n                    this.dialogm2 = true\n                    }, 300);\n                })\n            },\n            onNewClick() {\n                this.option = 1\n                this.clear()\n            },\n            async onSaveClose() {\n                if (!this.$refs.form.validate()) {\n                    this.$refs.form.validate()\n                    return\n                }\n                new Promise(resolve => {\n                    setTimeout(() => {\n                        resolve('resolved');\n                        let data = {\n                            \"id\": this.nssf.id ? this.nssf.id : '',\n                            \"name\": this.nssf.name,\n                            \"number\": this.nssf.number,\n                        }\n                        payrollHandler.createNSSF(data).then(response => {\n                            if (response.data.statusCode === 201) {\n                                const res = response.data\n                                const index = this.nssfList.findIndex(item => {\n                                    return (res.data.id === item.id)\n                                })\n                                if (this.nssfList.length === 0) {\n                                    this.loadNSSF()\n                                } else {\n                                    if (index < 0) {\n                                        this.nssfList.push(res.data)\n                                    } else {\n                                        this.nssfList.splice(index, 1, res.data)\n                                    }\n                                }\n                                this.$snotify.success('Success')\n                                this.clear()\n                                this.close()\n                            }\n                        }).catch(e => {\n                            this.$snotify.error('Something went wrong')\n                            this.errors.push(e)\n                        })\n                    }, 300);\n                })\n            },\n            async loadNSSF(){\n                new Promise(resolve => {\n                    setTimeout(() => {\n                        resolve('resolved');\n                        payrollHandler.getNSSF().then(res => {\n                            this.nssfList = res\n                        })\n                    }, 300);\n                })\n            },\n            close() {\n                this.dialogm2 = false\n            },\n            clear() {  \n                this.departmentSelected   = ''\n                this.nssf = {}\n            },\n        },\n        mounted: async function () {\n            this.loadNSSF()\n        }\n    };\n</script>\n<style scoped>\n    .acc_group header {\n        height: 10px !important;\n    }\n\n    table.acc_group tr td {\n        border-bottom: 1px solid #ccc;\n        padding: 8px;\n        vertical-align: middle;\n    }\n\n    table.acc_group tr td:first-child {\n        font-weight: 700;\n    }\n\n    table.acc_group tr td:last-child {\n        text-align: center;\n    }\n\n    .v-card__actions .v-btn.v-btn {\n        padding: 0 16px;\n    }\n\n    .exchange-table.theme--light.v-data-table > .v-data-table__wrapper > table > thead > tr > th {\n        font-family: 'Niradei-Bold', serif;\n        color: #000 !important;\n        border-top: 1px solid #000 !important;\n        border-bottom: 1px solid #000 !important;\n        font-size: 15px !important;\n    }\n\n    .exchange-table.theme--light.v-data-table > .v-data-table__wrapper > table > thead > tr > td {\n        color: #000 !important;\n        padding: 5px !important;\n    }\n\n    .exchange-table.theme--light.v-data-table > .v-data-table__wrapper > table > tbody > tr:last-child td {\n        border-bottom: 1px solid #000 !important;\n    }\n\n    .function_footer {\n        padding: 15px;\n        display: inline-block;\n    }\n\n    @media (max-width: 576px) {\n\n    }\n</style>"],"sourceRoot":"src/views/payroll/settings"}]}