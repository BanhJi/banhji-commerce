ge=\"Department\"\n                                                            v-model=\"classType\"\n                                                            item-value=\"uuid\"\n                                                            :loading=\"tLoading\"\n                                                            outlined\n                                                            return-object/>\n                                                    </v-col>\n                                                </v-row>\n                                                <v-row>\n                                                    <v-col sm=\"4\" class=\"pt-0 pr-0\">\n                                                        <p class=\"label font_16 mb-0\">{{ $t(\"type_no\") }}:</p>\n                                                    </v-col>\n                                                    <v-col sm=\"6\" class=\"pt-0 pl-0\">\n                                                        <v-text-field\n                                                            class=\"mt-1\"\n                                                            outlined\n                                                            v-model=\"classGroup.code\"\n                                                            id=\"Class type\"\n                                                            placeholder=\"Code\"\n                                                            :rules=\"[(v) => !!v || 'Number is required']\"/>\n                                                    </v-col>\n                                                </v-row>\n                                                <v-row>\n                                                    <v-col sm=\"4\" class=\"pt-0 pr-0\">\n                                                        <label class=\"label\">{{ $t(\"type_name\") }}</label>\n                                                    </v-col>\n                                                    <v-col sm=\"6\" class=\"pt-0 pl-0\">\n                                                        <v-text-field\n                                                            class=\"mt-1\"\n                                                            outlined\n                                                            v-model=\"classGroup.name\"\n                                                            id=\"Class Name\"\n                                                            placeholder=\"Name\"\n                                                            :rules=\"[(v) => !!v || 'Number is required']\"/>\n                                                    </v-col>\n                                                </v-row>\n                                            </v-col>\n                                        </v-row>\n                                    </v-form>\n                                </v-card-text>\n                                <v-divider/>\n                                <v-card-actions class=\"modal_footer\">\n                                    <v-row>\n                                        <v-col sm=\"6\" cols=\"6\" class=\"py-0 text-left\">\n                                            <v-btn color=\"black\"\n                                                outlined\n                                                class=\" text-capitalize  black--text float-left\"\n                                                @click=\"dialogM2 = false\"\n                                                >{{ $t(\"cancel\") }}</v-btn>\n                                        </v-col>\n                                        <v-col sm=\"6\" cols=\"6\" class=\"py-0 text-right\">\n                                            <v-btn @click=\"onSaveClose\"\n                                                :disabled=\"btnEnable\"\n                                                color=\"primary\"\n                                                class=\" white--text text-capitalize\">{{ $t(\"save_close\") }}\n                                            </v-btn>\n                                        </v-col>\n                                    </v-row>\n                                </v-card-actions>\n                            </v-card>\n                        </v-dialog>\n                        <p class=\"mb-0\">{{ $t(\"class_func_setting_desc\")}}</p>\n                    </v-col>\n                    <v-col sm=\"12\" cols=\"12\">\n                        <template>\n                        <div v-if=\"!isLoaded\">\n                            <LoadingMe\n                            :isLoading=\"showLoading\"\n                            :fullPage=\"false\"\n                            :myLoading=\"true\"\n                            type=\"loading\"/>\n                        </div>\n                        <kendo-datasource ref=\"groupDS\"\n                            :data=\"classGroups\"\n                            :sort=\"sort\"\n                            :group=\"group\"/>\n                        <kendo-grid id=\"gridGroup\"\n                            class=\"grid-function\"\n                            ref=\"gridComponent\"\n                            :data-source-ref=\"'groupDS'\"\n                            :pageable=\"true\"\n                            :groupable=\"true\"\n                            :editable=\"'inline'\"\n                            :scrollable-virtual=\"true\">\n                            <kendo-grid-column\n                                :field=\"'code'\"\n                                :title=\"$t('code')\"\n                                :width=\"100\"\n                                :template=\"'<span>#=code#</span>'\"\n                                :headerAttributes=\"{ style: 'background-color: #EDF1F5'}\"/>\n                            <kendo-grid-column\n                                :field=\"'name'\"\n                                :attributes=\"{ class: 'tb_name_td' }\"\n                                :title=\"$t('name')\"\n                                :template=\"'<span>#=name#</span>'\"\n                                :headerAttributes=\"{ style: 'background-color: #EDF1F5'}\"/>\n                            <kendo-grid-column\n                                :field=\"'clazz.code'\"\n                                :title=\"$t('nature')\"\n                                :template=\"'<span>#=clazz.code#- #=clazz.name#</span>'\"\n                                :group-header-template=\"'#=value#'\"\n                                :headerAttributes=\"{ style: 'background-color: #EDF1F5'}\"/>\n                            <kendo-grid-column\n                                :field=\"'classType.code'\"\n                                :title=\"$t('type')\"\n                                :template=\"'<span>#=classType.code#- #=classType.name#</span>'\"\n                                :headerAttributes=\"{ style: 'background-color: #EDF1F5'}\"/>\n                            <kendo-grid-column\n                                :field=\"''\"\n                                :title=\"$t('action')\"\n                                :width=\"120\"\n                                :command=\"[{iconClass: 'k-icon k-i-edit', text: 'Edit',click: goEdit}]\"\n                                :headerAttributes=\"{style: 'text-align: center; background-color: #EDF1F5'}\"\n                                :attributes=\"{ style: 'text-align: center' }\"/>\n                        </kendo-grid>\n                        </template>\n                    </v-col>\n                </v-row>\n            </v-card>\n        </v-col>\n    </v-row>\n</template>\n\n<script>\nimport kendo from \"@progress/kendo-ui\";\nconst $ = require(\"jquery\");\nconst settingsHandler = require(\"@/scripts/settingsHandler\");\nimport ClassGroupModel from \"@/scripts/model/Clazz\";\nconst classGroupModel = new ClassGroupModel({});\n\nexport default {\n    components: {\n        LoadingMe: () => import(`@/components/Loading`),\n    },\n    data: () => ({\n        dialogM2: false,\n        valid: false,\n        showLoading: true,\n        isLoaded: false,\n        hidden: false,\n        clazzes: [],\n        clazz: {},\n        classGroup: classGroupModel,\n        classGroups: [],\n        clazzTypes: [],\n        classType: {},\n        schemaDefinition: {\n            model: { id: \"id\" },\n        },\n        btnEnable: false,\n        cLoading: true,\n        tLoading: true,\n        sort: [{ field: \"code\", dir: \"ASC\" }],\n        group: { field: \"clazz.code\" },\n    }),\n    props: {},\n    computed: {\n        dialogM4: function() {\n            return this.dialogM3;\n        },\n    },\n    mounted: async function() {\n        await this.loadGroup()\n    },\n    methods: {\n        close() {\n            this.classGroup     = new ClassGroupModel();\n            this.clazzes        = []\n            this.clazz          = {}\n            this.clazzTypes     = []\n            this.classType      = {}\n            this.dialogM2       = false;\n        },\n        goEdit(e) {\n            e.preventDefault();\n            let grid        = kendo.jQuery(\"#gridGroup\").data(\"kendoGrid\");\n            let dataItem    = grid.dataItem($(e.currentTarget).closest(\"tr\"));\n            window.console.log('dataItem', dataItem)\n            this.classGroup = new ClassGroupModel(dataItem);\n            this.clazz      = {\n                code:           dataItem.clazz.code,\n                id:             dataItem.clazz.id,\n                isShow:         dataItem.clazz.isShow,\n                isSystem:       dataItem.clazz.isSystem,\n                name:           dataItem.clazz.name,\n            }\n            this.classType  = {\n                clazz:          dataItem.classType.clazz,\n                clazzId:        dataItem.classType.clazzId,\n                code:           dataItem.classType.code,\n                id:             dataItem.classType.id,\n                isShow:         dataItem.classType.isShow,\n                isSystem:       dataItem.classType.isSystem,\n                name:           dataItem.classType.name,\n            }\n            this.loadClass()\n            this.changeClass()\n            this.dialogM2 = true;\n        },\n        async loadClass() {\n            let params = {\n                type:       'class'\n            }\n            new Promise((resolve) => {\n                setTimeout(() => {\n                resolve(\"resolved\");\n                this.clazzes = [];\n                settingsHandler.getClazz(params).then((res) => {\n                    this.showLoading = true;\n                    if (res.data.statusCode === 200) {\n                        this.showLoading = false;\n                        this.clazzes = res.data.data;\n                        if(this.clazzes.length > 0){\n                            this.cLoading = false\n                        }\n                    }\n                }).catch();\n                {\n                    this.showLoading = false;\n                }\n                }, 300);\n            });\n        },\n        async loadType(clazzId) {\n            let params = {\n                type:       'class_type',\n                clazzId:    clazzId\n            }\n            new Promise((resolve) => {\n                setTimeout(() => {\n                    resolve(\"resolved\");\n                    this.clazzTypes = [];\n                    settingsHandler.getClazz(params).then((res) => {\n                        this.showLoading = true;\n                        if (res.data.statusCode === 200) {\n                            this.showLoading = false;\n                            this.clazzTypes = res.data.data;\n                            if(this.clazzTypes.length > 0){\n                                this.tLoading = false\n                            }\n                        }\n                    }).catch();\n                    {\n                        this.showLoading = false;\n                    }\n                }, 300);\n            });\n        },\n        async loadGroup() {\n            let params = {\n                type:       'class_group',\n            }\n            new Promise((resolve) => {\n                setTimeout(() => {\n                    resolve(\"resolved\");\n                    this.clazzTypes = [];\n                    settingsHandler.getClazz(params).then((res) => {\n                        this.showLoading = true;\n                        if (res.data.statusCode === 200) {\n                            this.showLoading = false;\n                            this.classGroups = res.data.data;\n                        }\n                    }).catch();\n                    {\n                        this.showLoading = false;\n                    }\n                }, 300);\n            });\n        },\n        async onSaveClose() {\n            if (!this.$refs.form.validate()) {\n                this.$refs.form.validate()\n                return\n            }\n            new Promise((resolve) => {\n                setTimeout(() => {\n                    resolve(\"resolved\");\n                    let data = new ClassGroupModel({\n                        id:             this.classGroup.id ? this.classGroup.id : \"\",\n                        name:           this.classGroup.name,\n                        code:           this.classGroup.code,\n                        isShow:         this.classGroup.isShow,\n                        clazz:          this.clazz || {},\n                        clazzId:        this.clazz.id || '',\n                        classType:      this.classType || {},\n                        classTypeId:    this.classType.id || '',\n                        type:           \"class_group\"\n                    });\n                    this.btnEnable = true;\n                    window.console.log('data', data)\n                    settingsHandler.create(data).then((response) => {\n                        if (response.data.statusCode === 201) {\n                            const res = response.data.data;\n                            const index = this.classGroups.findIndex((item) => {\n                                return res.id === item.id;\n                            });\n                            if (this.classGroups.length === 0) {\n                                this.loadGroup();\n                            } else {\n                            if (index < 0) {\n                                this.classGroups.push(res);\n                            } else {\n                                this.classGroups.splice(index, 1, res);\n                            }\n                            }\n                            this.btnEnable = false;\n                            this.$snotify.success(\"Success\");\n                            this.close();\n                        }\n                    }).catch((e) => {\n                        this.btnEnable = false;\n                        this.$snotify.error(\"Something went wrong\");\n                        this.errors.push(e);\n                    });\n                }, 300);\n            });\n        },\n        onCreateNew(){\n            this.loadClass()\n            this.dialogM2 = true;\n        },\n        changeClass(){\n            window.console.log(121212121, this.clazz)\n            this.loadType(this.clazz.id)\n        }\n    },\n};\n</script>\n<style scoped>\n    .acc_group header {\n        height: 10px !important;\n    }\n    table.acc_group tr td {\n        border-bottom: 1px solid #ccc;\n        padding: 8px;\n        vertical-align: middle;\n    }\n    table.acc_group tr td:first-child {\n        font-weight: 700;\n    }\n\n    table.acc_group tr td:last-child {\n        text-align: center;\n    }\n    .v-card__actions .v-btn.v-btn {\n        padd