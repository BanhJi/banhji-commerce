,WA1DA,yBA0DA;AAAA;;AACA,WAAA,YAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA,WAAA,CAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA;AAIA,KAhEA;AAiEA,IAAA,YAjEA,0BAiEA;AACA,UAAA,KAAA,GAAA,IAAA;;AAEA,UAAA,KAAA,UAAA,CAAA,OAAA,CAAA,IAAA,KAAA,EAAA,EAAA;AACA,QAAA,KAAA,GAAA,KAAA;AAEA,aAAA,QAAA,CAAA,OAAA,CAAA,IAAA,CAAA,CAAA,CAAA,qBAAA,CAAA;AACA;;AAEA,aAAA,KAAA;AACA,KA3EA;AA4EA,IAAA,IA5EA,kBA4EA;AAAA;;AACA,UAAA,KAAA,YAAA,EAAA,EAAA;AACA,aAAA,WAAA,GAAA,IAAA;AACA,QAAA,oBAAA,CAAA,IAAA,CAAA,KAAA,UAAA,EACA,IADA,CACA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,IAAA,CAAA,CAAA,CAAA,iBAAA,CAAA;AACA,SAHA,EAIA,OAJA,CAIA,YAAA;AACA,UAAA,MAAA,CAAA,WAAA,GAAA,KAAA;AACA,SANA;;AAQA,YAAA,KAAA,WAAA,GAAA,CAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,KAAA,cAAA,CAAA,KAAA,WAAA,CAAA,EAAA,KAAA,UAAA;AACA,SAFA,MAEA;AACA,eAAA,cAAA,CAAA,IAAA,CAAA,KAAA,UAAA;AACA;;AACA,aAAA,KAAA;AACA;AACA;AA9FA,GAzBA;AAyHA,EAAA,QAAA,EAAA,QAAA,CAAA;AACA,IAAA,cAAA,EAAA,wBAAA,KAAA;AAAA,aAAA,KAAA,CAAA,UAAA,CAAA,cAAA;AAAA;AADA,GAAA,CAzHA;AA4HA,EAAA,OA5HA,qBA4HA;AACA,SAAA,UAAA;AACA,SAAA,OAAA;AACA;AA/HA,CAAA","sourcesContent":["<template>\n  <v-data-table\n    :headers=\"headers\"\n    :items=\"reconcileRules\"\n    sort-by=\"calories\"\n    class=\"elevation-1\"\n  >\n    <template v-slot:top>\n        <v-toolbar\n            flat\n        >\n            <v-toolbar-title>Rules</v-toolbar-title>\n            <v-divider\n            class=\"mx-4\"\n            inset\n            vertical\n            ></v-divider>\n            \n            <v-spacer></v-spacer>\n\n            <!-- Form Dialog -->\n            <v-dialog\n            v-model=\"dialog\"\n            max-width=\"500px\"\n            >\n                <!-- Create New Button -->\n                <template v-slot:activator=\"{ on, attrs }\">\n                    <v-btn\n                        color=\"primary\"\n                        dark\n                        class=\"mb-2 mx-2\"\n                        @click=\"refresh\"\n                    >\n                        <v-icon color=\"white\">mdi-reload</v-icon>\n                    </v-btn>\n\n                    <v-btn\n                        color=\"primary\"\n                        dark\n                        class=\"mb-2\"\n                        v-bind=\"attrs\"\n                        v-on=\"on\"\n                    >\n                        {{ $t(\"create_new\") }}\n                    </v-btn>\n                </template>\n\n                <!-- Form -->\n                <v-card>\n                    <div class=\"modal_header\">\n                        <v-card-title>{{ $t(\"rule\") }}</v-card-title>\n                        <v-icon  @click=\"close\">close</v-icon>\n                    </div>\n                    <v-card-text class=\"modal_text_content\">\n                        <v-row class=\"pt-2\">\n                            <v-col sm=\"9\" cols=\"12\" class=\"py-0\">\n                                <label class=\"label\">{{ $t(\"account\") }}</label>\n                                <div v-if=\"editedIndex === -1\">\n                                    <app-account-dropdownlist \n                                        :initAccount=\"editedItem.account\" \n                                        @emitAccount=\"editedItem.account = $event\" />\n                                </div>\n                                <div v-else>\n                                    <v-text-field\n                                        v-model=\"editedItem.account.name\"\n                                        readonly\n                                        outlined\n                                    ></v-text-field>\n                                </div>\n                            </v-col>\n                            <v-col sm=\"3\" cols=\"12\" class=\"py-0\">\n                                <label class=\"label\">{{\n                                    $t(\"todo_every\")\n                                }}</label>\n                                <v-select\n                                    :type=\"text\"\n                                    :items=\"days\"\n                                    v-model=\"editedItem.dayOfTheMonth\"\n                                    outlined\n                                ></v-select>\n                            </v-col>\n                            <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                                <label class=\"label\">{{ $t(\"instruction\") }}</label>\n                                <v-textarea\n                                    class=\"\"\n                                    outlined\n                                    v-model=\"editedItem.note\"\n                                    no-resize\n                                    height=\"110px\"\n                                    rows=\"3\"\n                                />\n                            </v-col>\n                        </v-row>\n                    </v-card-text>\n\n                    <v-card-actions>\n                    <v-spacer></v-spacer>\n                    <v-btn\n                        color=\"blue darken-1\"\n                        text\n                        @click=\"close\"\n                    >\n                        Cancel\n                    </v-btn>\n                    <v-btn\n                        color=\"blue darken-1\"\n                        text\n                        @click=\"save\"\n                    >\n                        Save\n                    </v-btn>\n                    </v-card-actions>\n                </v-card>\n            </v-dialog>\n\n            <!-- Delete Dialog -->\n            <v-dialog v-model=\"dialogDelete\" max-width=\"500px\">\n            <v-card>\n                <v-card-title class=\"text-h5\">Are you sure you want to delete this item?</v-card-title>\n                <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn color=\"blue darken-1\" text @click=\"closeDelete\">Cancel</v-btn>\n                <v-btn color=\"blue darken-1\" text @click=\"deleteItemConfirm\">OK</v-btn>\n                <v-spacer></v-spacer>\n                </v-card-actions>\n            </v-card>\n            </v-dialog>\n\n        </v-toolbar>\n    </template>\n\n    <!-- Account Template -->\n    <template v-slot:[`item.account`]=\"{ item }\">\n        {{ item.account.name }}\n    </template>\n\n    <!-- Edit & Delete -->\n    <template v-slot:[`item.actions`]=\"{ item }\">\n      <v-icon\n        small\n        class=\"mr-2\"\n        @click=\"editItem(item)\"\n      >\n        mdi-pencil\n      </v-icon>\n      <v-icon\n        small\n        @click=\"deleteItem(item)\"\n      >\n        mdi-delete\n      </v-icon>\n    </template>\n\n    <!-- No Data -->\n    <template slot=\"no-data\">\n        <LoadingMe\n            :isLoading=\"showLoading\"\n            :fullPage=\"false\"\n            :myLoading=\"true\"\n            type=\"loading\"\n        />\n    </template>\n  </v-data-table>\n</template>\n\n<script>\nimport {i18n} from '@/i18n';\nimport kendo from \"@progress/kendo-ui\";\nimport ReconcileRuleModel from \"@/scripts/model/ReconcileRule\";\n// import AccountModel from \"@/scripts/model/Account\";\nimport store from \"@/store\";\nimport { mapState } from 'vuex';\n\nconst ReconcileRuleHandler = require(\"@/scripts/handler/accounting/reconcileRule\");\n\nexport default {\n    components: {\n        \"LoadingMe\": () => import('@/components/Loading'),\n        \"app-account-dropdownlist\": () => import('@/components/dropdownlist/Account'),\n    },\n    data: () => ({\n        showLoading : false,\n        dialog: false,\n        dialogDelete: false,\n        days : [],\n        headers: [\n            {\n                text: 'Account',\n                align: 'start',\n                sortable: false,\n                value: 'account',\n            },\n            { text: 'Todo Every', value: 'dayOfTheMonth' },\n            { text: 'Instruction', value: 'note' },\n            { text: 'Actions', value: 'actions', sortable: false },\n        ],\n        editedIndex: -1,\n\t\teditedItem: new ReconcileRuleModel(),\n\t\tdefaultItem: new ReconcileRuleModel(),\n    }),\n    methods: {\n        async initialize () {\n            this.showLoading = true;\n\n            store.state.accounting.reconcileRuleLoaded = false;\n            \n            // Call List\n            await this.$store.dispatch('accounting/getReconcileRules');\n\n            /* Parse Day Of The Month to Integer */\n            this.reconcileRules.forEach(value => {\n                value.dayOfTheMonth = kendo.parseInt(value.dayOfTheMonth);\n            });\n\n            this.showLoading = false;\n        },\n        setDays () {\n            if(this.days.length === 0){\n                for (let i = 1; i <= 31; i++) {\n                    this.days.push(i);\n                }\n            }\n        },\n        refresh(){\n            this.initialize();\n        },\n        editItem (item) {\n            this.editedIndex = this.reconcileRules.indexOf(item)\n            this.editedItem = Object.assign({}, item)\n            this.dialog = true\n        },\n        deleteItem (item) {\n            this.editedIndex = this.reconcileRules.indexOf(item)\n            this.editedItem = Object.assign({}, item)\n            this.dialogDelete = true\n        },\n        deleteItemConfirm () {\n            this.showLoading = true;\n            ReconcileRuleHandler.delete({\n\t\t\t\t\tdata: this.editedItem\n\t\t\t\t})\n                .then(()=>{\n                    this.$snotify.success(i18n.t(\"delete_successful\"));\n                })\n                .finally(()=>{\n                    this.showLoading = false;\n                });\n\n            this.reconcileRules.splice(this.editedIndex, 1)\n            this.closeDelete()\n        },\n        close () {\n            this.dialog = false\n            this.$nextTick(() => {\n                this.editedItem = Object.assign({}, this.defaultItem)\n                this.editedIndex = -1\n            })\n        },\n        closeDelete () {\n            this.dialogDelete = false\n            this.$nextTick(() => {\n                this.editedItem = Object.assign({}, this.defaultItem)\n                this.editedIndex = -1\n            })\n        },\n        validateForm(){\n            let valid = true;\n\n            if(this.editedItem.account.uuid === \"\"){\n                valid = false;\n\n                this.$snotify.warning(i18n.t(\"account_is_required\"));\n            }\n\n            return valid;\n        },\n        save () {\n            if(this.validateForm()){\n                this.showLoading = true;\n                ReconcileRuleHandler.save(this.editedItem)\n                    .then(()=>{\n                        this.$snotify.success(i18n.t(\"save_successful\"));\n                    })\n                    .finally(()=>{\n                        this.showLoading = false;\n                    });\n\n                if (this.editedIndex > -1) {\n                    Object.assign(this.reconcileRules[this.editedIndex], this.editedItem)\n                } else {\n                    this.reconcileRules.push(this.editedItem)\n                }\n                this.close()\n            }\n        },\n    },\n    computed: mapState({\n        reconcileRules: state => state.accounting.reconcileRules\n    }),\n    created () {\n      this.initialize();\n      this.setDays();\n    }\n};\n</script>\n<style scoped>\n    .theme--light.v-data-table > .v-data-table__wrapper > table > tbody > tr:first-child > td:not(.v-data-table__mobile-row) {\n        border-top: 3px solid rgba(0, 0, 0, 0.12) !important;\n    }\n\n    .float-right {\n        margin-top: 0px !important;\n    }\n\n    .float-left {\n        margin-top: 0px !important;\n\n    }\n\n    .v-input--switch {\n        margin-top: 0px;\n        height: 30px;\n    }\n\n    .lightBlue {\n        color: #68cbfb;\n    }\n\n\n</style>"],"sourceRoot":"src/views/accounting/closing"}]}