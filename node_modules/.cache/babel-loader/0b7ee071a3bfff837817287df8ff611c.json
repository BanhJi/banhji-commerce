t\t\t\t\t\t\t<span class=\"hidden-sm-and-up\">\n\t\t\t\t\t\t\t\t<v-icon left>mdi-pen</v-icon>\n\t\t\t\t\t\t\t</span>\n                            <span class=\"hidden-sm-and-down\">\n\t\t\t\t\t\t\t\t{{$t('attachments')}}\n\t\t\t\t\t\t\t</span>\n                        </v-tab>\n\n                        <v-tab-item class=\"grayBg\" >\n                            <v-row>\n                                <v-col sm=\"12\" cols=\"12\" class=\"pl-3\" >\n                                   <Info :initBankAccount=\"bankAccount\" />\n                                </v-col>\n                            </v-row>\n                        </v-tab-item>\n\n                        <v-tab-item class=\"grayBg\" >\n                            <v-row>\n                                <v-col sm=\"12\" cols=\"12\" class=\"pl-3\">\n                                    <Statement :initBankAccount=\"bankAccount\" />\n                                </v-col>\n                            </v-row>\n                        </v-tab-item>\n\n                        <!-- Transaction -->\n                        <v-tab-item class=\"grayBg\" >\n                            <v-row>\n                                <v-col sm=\"12\" cols=\"12\" class=\"pl-3\">\n                                    <Txn :initBankAccount=\"bankAccount\" />\n                                </v-col>\n                            </v-row>\n                        </v-tab-item>\n                        <!-- Attachment -->\n                        <v-tab-item class=\"grayBg\" >\n                            <v-row>\n                                <v-col sm=\"12\" cols=\"12\" class=\"pl-3\">\n                                    <Attachment :initAccount=\"attachment\"/>\n                                </v-col>\n                            </v-row>\n                        </v-tab-item>\n\n                    </v-tabs>\n                </v-col>\n            </v-row>\n        </v-col>\n\n    </v-row>\n\n</template>\n\n<script>\n  