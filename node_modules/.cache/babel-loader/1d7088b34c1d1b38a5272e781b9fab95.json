\n                                                            :initEndDate=\"endDate\" @emitEndDate=\"endDate = $event\"/>\n                                        </v-col>\n                                        <v-col sm=\"1\" cols=\"12\" class=\"py-1\">\n                                            <v-btn class=\"btn_search white--text\" color=\"primary\" >\n                                                <i class=\"b-search\" style=\"font-size: 18px; color:#fff !important;\"/>\n                                            </v-btn>\n                                        </v-col>\n<!--                                        <v-col sm=\"2\" cols=\"12\" class=\"py-1\">-->\n<!--                                            <v-btn class=\"float-right capitalize white&#45;&#45;text\" color=\"primary\" >-->\n<!--                                                {{$t('add_new')}}-->\n<!--                                            </v-btn>-->\n<!--                                        </v-col>-->\n                                    </v-row>\n                                </v-col>\n                            </v-row>\n\n                            <v-row class=\"\">\n                                <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                                    <kendo-datasource ref=\"dataSource\"\n                                                      :data=\"withholdings\"\n                                    />\n                                    <kendo-grid id=\"gridReceivable\" class=\"grid-function\"\n                                                :data-source-ref=\"'dataSource'\"\n                                                :sortable=\"false\"\n                                                :groupable=\"false\"\n                                                :filterable=\"false\"\n                                                :column-menu=\"true\"\n                                                :editable=\"false\"\n                                                :noRecords=\"true\"\n                                                :scrollable-virtual=\"true\"\n                                    >\n                                        <kendo-grid-column\n                                            :field=\"'reference'\"\n                                            :title=\"$t('num')\"\n                                            :template=\"'<span>#=reference#</span>'\"\n                                            :headerAttributes=\"{\n                                                                    style: 'background-color: #EDF1F5'\n                                                                }\"/>\n                                        <kendo-grid-column\n                                            :field=\"'date'\"\n                                            :title=\"$t('date')\"\n                                            :template=\"'<span>#=kendo.toString(new Date(date), \\'MM-yyyy\\')#</span>'\"\n                                            :headerAttributes=\"{\n                                                                    style: 'background-color: #EDF1F5'\n                                                                }\"/>\n                                        <kendo-grid-column\n                                            :field=\"'monthOf'\"\n                                            :title=\"$t('month')\"\n                                            :hidden=\"true\"\n                                            :template=\"'<span>#=monthOf#</span>'\"\n                                            :headerAttributes=\"{\n                                                                    style: 'background-color: #EDF1F5'\n                                                                }\"/>\n                                        <kendo-grid-column\n                                            :field=\"'supplier'\"\n                                            :title=\"$t('id')\"\n                                            :template=\"'<span>#=supplier.supplierId#</span>'\"\n                                            :headerAttributes=\"{\n                                                                    style: 'background-color: #EDF1F5'\n                                                                }\"/>\n                                        <kendo-grid-column\n                                            :field=\"'supplier'\"\n                                            :title=\"$t('name')\"\n                                            :attributes=\"{class:'text-bold'}\"\n                                            :template=\"'<span>#=supplier.name#</span>'\"\n                                            :headerAttributes=\"{\n                                                                    style: 'background-color: #EDF1F5'\n                                                                }\"/>\n                                        <kendo-grid-column\n                                            :field=\"'natureTaxType'\"\n                                            :title=\"$t('nature_of_wht')\"\n                                            :template=\"'<span>#=supplier.natureTaxType#</span>'\"\n                                            :headerAttributes=\"{\n                                                                    style: 'background-color: #EDF1F5'\n                                                                }\"/>\n                                        <kendo-grid-column\n                                            :field=\"'totalAmount'\"\n                                            :hidden=\"true\"\n                                            :template=\"'<span>#= kendo.toString(totalAmount, `n2`)#</span>'\"\n                                            :title=\"$t('amount')\"\n                                            :headerAttributes=\"{\n                                                                    style: 'background-color: #EDF1F5'\n                                                                }\"/>\n                                        <kendo-grid-column\n                                            :field=\"'totalVAT'\"\n                                            :title=\"$t('amount_wht')\"\n                                            :template=\"'<span>#= kendo.toString(totalVAT, `n2`)#</span>'\"\n                                            :headerAttributes=\"{\n                                                                    style: 'background-color: #EDF1F5'\n                                                                }\"/>\n                                        <kendo-grid-column\n                                            :field=\"'dataSource'\"\n                                            :title=\"$t('source')\"\n                                            :hidden=\"true\"\n                                            :template=\"'<span>#=dataSource.name#</span>'\"\n                                            :headerAttributes=\"{\n                                                                    style: 'background-color: #EDF1F5'\n                                                                }\"/>\n                                        <kendo-grid-column\n                                            :field=\"'status'\"\n                                            :title=\"$t('status')\"\n                                            :template=\"'<span>#=status.name#</span>'\"\n                                            :hidden=\"true\"\n                                            :headerAttributes=\"{\n                                                                    style: 'background-color: #EDF1F5'\n                                                                }\"/>\n\n                                    </kendo-grid>\n                                    <LoadingMe\n                                        :isLoading=\"showLoading\"\n                                        :fullPage=\"false\"\n                                        :myLoading=\"true\"\n                                    />\n                                </v-col>\n                            </v-row>\n                        </v-col>\n                    </v-row>\n                </v-col>\n            </v-card>\n        </v-col>\n    </v-row>\n</template>\n<script>\nimport LoadingMe from \"@/components/Loading\";\nconst withholdingHandler = require(\"@/scripts/compliance/handler/withholdingTaxHandler\")\nexport default {\n    data: () => ({\n        showLoading: false,\n        withholdings: [],\n        is_month: false,\n        is_start_date: false,\n        is_end_date: false,\n        startDate: \"\",\n        endDate: \"\",\n\n        search:{\n            start_date: \"\",\n            end_date: \"\"\n        },\n\n    }),\n    props: {},\n    methods: {\n        async onloadWithholding() {\n            this.showLoading = true\n            await withholdingHandler.getAll().then(res => {\n                this.withholdings = res.data.data\n                this.showLoading = false\n                window.console.log(res, 'withholding limit')\n            })\n        },\n        async onSearch(){\n            this.showLoading = true\n            this.search = {\n                'start_date': this.startDate,\n                'end_date': this.endDate\n            }\n            await withholdingHandler.getBetweenMonth(this.search).then(res => {\n                this.withholdings = res.data.data\n                this.showLoading = false\n                window.console.log(res, 'withholding between')\n            })\n        }\n    },\n    async mounted() {\n        await this.onloadWithholding()\n    },\n    computed: {},\n    components: {\n        'LoadingMe': LoadingMe,\n        'app-searchdate' : ()=> import('@/components/custom_templates/SearchDate'),\n        // 'month-picker' : ()=> import('@/components/custom_templates/MonthPicker')\n    },\n};\n</script>\n<style scoped>\n    .theme--light.v-data-table > .v-data-table__wrapper > table > tbody > tr:hover:not(.v-data-table__expanded__content):not(.v-data-table__empty-wrapper) {\n        background-color: transparent !important;\n    }\n    .theme--light.v-data-table > .v-data-table__wrapper > table > thead > tr:last-child > th {\n        font-size: 14px;\n        border-bottom: thin solid rgba(0, 0, 0, 0.12);\n        height: 37px !important;\n        color: #000;\n    }\n    .theme--light.v-data-table > .v-data-table__wrapper > table > tbody > tr.primary:hover:not(.v-data-table__expanded__content):not(.v-data-table__empty-wrapper) {\n        background-color: #92d050 !important;\n    }\n\n    .theme--light.v-data-table {\n        background-color: transparent !important;\n        border-top: thin solid #ddd;\n        border-bottom: thin solid #ddd;\n        border-radius: 0 !important;\n    }\n\n    .theme--light.v-data-table > .v-data-table__wrapper > table > tbody > tr:not(:last-child) > td:not(.v-data-table__mobile-row),\n    .theme--light.v-data-table > .v-data-table__wrapper > table > tbody > tr:not(:last-child) > th:not(.v-data-table__mobile-row) {\n        border-bottom: thin solid #ddd;\n    }\n\n    .v-data-table > .v-data-table__wrapper > table > tbody > tr > td {\n        height: 32px !important;\n        border-bottom: thin solid rgba(0, 0, 0, 0.12) !important;\n    }\n    .border_top {\n        border-top: thin solid #fff !important;\n    }\n\n    .v-application .primary,\n    .v-application .third {\n        border-color: #fff !important;\n    }\n    .vat{\n        text-transform: uppercase;\n        color: #fff;\n        font-weight: bold;\n        font-family: 'Niradei-Bold' !important;\n    }\n\n    .amount {\n        text-align: right !important;\n    }\n    .head-summary {\n        text-align: right !important;\n    }\n</style>\n"],"sourceRoot":"src/views/compliance/monthly_tax/records"}]}