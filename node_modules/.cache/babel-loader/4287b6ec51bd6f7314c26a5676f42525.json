 !== '') {\n            let response = await instituteHandler.init();\n            commit(\"setInstitute\", response.data.data);\n        } else {\n            commit(\"setInstitute\", defaultData);\n        }\n    },\n};\n\n// mutations\nconst mutations = {\n    setInstitute(state, value) {\n        state.institute = value;\n    },\n}\n\nexport default {\n    namespaced: true,\n    state,\n    getters,\n    actions,\n    mutations\n}"]}]}