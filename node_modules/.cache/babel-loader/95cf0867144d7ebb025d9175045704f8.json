v>\n                      <div class=\"d-flex\">\n                        <v-checkbox\n                          @change=\"UpdateFormChange\"\n                          class=\"mt-0 pt-0\"\n                          v-model=\"invoice_setting.column.ten2\"\n                        />\n                        <label>{{ $t(\"discount\") }}</label>\n                      </div>\n                      <div class=\"d-flex\">\n                        <v-checkbox\n                          @change=\"UpdateFormChange\"\n                          class=\"mt-0 pt-0\"\n                          v-model=\"invoice_setting.column.ten3\"\n                        />\n                        <label>{{ $t(\"vat\") }}</label>\n                      </div>\n                      <div class=\"d-flex\">\n                        <v-checkbox\n                          @change=\"UpdateFormChange\"\n                          class=\"mt-0 pt-0\"\n                          v-model=\"invoice_setting.column.ten4\"\n                        />\n                        <label>{{ $t(\"specific_tax\") }}</label>\n                      </div>\n                      <div class=\"d-flex\">\n                        <v-checkbox\n                          @change=\"UpdateFormChange\"\n                          class=\"mt-0 pt-0\"\n                          v-model=\"invoice_setting.column.ten5\"\n                        />\n                        <label>{{ $t(\"other_tax\") }}</label>\n                      </div>\n                      <div class=\"d-flex\">\n                        <v-checkbox\n                          @change=\"UpdateFormChange\"\n                          class=\"mt-0 pt-0\"\n                          v-model=\"invoice_setting.column.ten6\"\n                        />\n                        <label>{{ $t(\"pl_tax\") }}</label>\n                      </div>\n                      <div class=\"d-flex\">\n                        <v-checkbox\n                          @change=\"UpdateFor