kendo-grid-column\n              :field=\"'annual_leave_amount'\"\n              :title=\"$t('annual')\"\n              :width=\"100\"\n              :template=\"\n                '<span>#= annual_leave_amount ? annual_leave_amount: 0#</span>'\n              \"\n              :headerAttributes=\"{\n                style: 'background-color: #EDF1F5',\n              }\"\n            />\n            <kendo-grid-column\n              :field=\"'maternity_leave_amount'\"\n              :title=\"$t('maternity')\"\n              :width=\"100\"\n              :hidden=\"true\"\n              :template=\"\n                '<span>#= maternity_leave_amount ? maternity_leave_amount: 0#</span>'\n              \"\n              :headerAttributes=\"{\n                style: 'background-color: #EDF1F5',\n              }\"\n            />\n            <kendo-grid-column\n              :field=\"'paternity_leave_amount'\"\n              :title=\"$t('paternity')\"\n              :width=\"100\"\n              :hidden=\"true\"\n              :template=\"\n                '<span>#= paternity_leave_amount ? paternity_leave_amount: 0#</span>'\n              \"\n              :headerAttributes=\"{\n                style: 'background-color: #EDF1F5',\n              }\"\n            />\n            <kendo-grid-column\n              :field=\"'study_leave_amount'\"\n              :title=\"$t('study')\"\n              :width=\"100\"\n              :hidden=\"true\"\n              :template=\"\n                '<span>#= study_leave_amount ? study_leave_amount: 0#</span>'\n              \"\n              :headerAttributes=\"{\n                style: 'background-color: #EDF1F5',\n              }\"\n            />\n            <kendo-grid-column\n              :field=\"'conpensat_leave_amount'\"\n              :title=\"$t('conpensat')\"\n              :width=\"100\"\n              :hidden=\"true\"\n              :template=\"\n                '<span>#= conpensat_leave_amount ? conpensat_leave_amount: 0#<