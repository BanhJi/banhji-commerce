l sm=\"12\" cols=\"12\" class=\"pt-4\">\n                <v-btn icon color=\"black\" class=\"bg-none float-right \">\n                  <v-icon class=\"font_34\">fa fa-file-excel</v-icon>\n                </v-btn>\n\n                <v-btn icon color=\"black\" class=\"bg-none float-right ml-2\">\n                  <v-icon class=\"font_34\">fa fa-print</v-icon>\n                </v-btn>\n              </v-col>\n            </v-row>\n\n          </v-col> -->\n        </v-row>\n        <v-row>\n          <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n            <template>\n              <LoadingMe\n                  :isLoading=\"showLoading\"\n                  :fullPage=\"false\"\n                  type=\"loading\"\n                  :myLoading=\"true\">\n\n              </LoadingMe>\n              <kendo-datasource ref=\"dataSource\"\n                                :data=\"receivableList\"\n              />\n              <kendo-grid id=\"gridReceivable\" class=\"grid-function\"\n                          :data-source-ref=\"'dataSource'\"\n                          :sortable=\"false\"\n                          :filterable=\"false\"\n                          :column-menu=\"true\"\n                          :editable=\"false\"\n                          :resizable=\"true\"\n                          :style=\"{width: '100%'}\"\n                          :noRecords=\"true\"\n                          :scrollable-virtual=\"true\"\n              >\n                <kendo-grid-column\n                    :field=\"'name'\"\n                    :attributes=\"{class:'tb_name_td'}\"\n                    :title=\"$t('name')\"\n                    :template=\"'<span>#=name#</span>'\"\n                    :width=\"200\"\n                    :headerAttributes=\"{\n                                                        style: 'background-color: #EDF1F5'\n                                                    }\"/>\n                <kendo-grid-column\n                    :field=\"'buom'\"\n                    :title=\"$t('base_uom')\"\n                    :width=\"150\"\n                    :template=\"'<span>#=buom.name || ``#</span>'\"\n                    :headerAttributes=\"{\n                                                        style: 'background-color: #EDF1F5'\n                                                    }\"/>\n                <kendo-grid-column\n                    :field=\"'committedStock'\"\n                    :title=\"$t('committed')\"\n                    :width=\"150\"\n                    :attributes=\"{style: 'text-align: right; '}\"\n                    :template=\"'<span>#=kendo.toString(committedStock, decimalFormat)#</span>'\"\n                    :headerAttributes=\"{\n                                                        style: 'background-color: #EDF1F5'\n                                                    }\"/>\n                <!--                <kendo-grid-column-->\n                <!--                    :field=\"'po'\"-->\n                <!--                    :title=\"$t('on_po')\"-->\n                <!--                    :width=\"150\"-->\n                <!--                    :attributes=\"{style: 'text-align: right; '}\"-->\n                <!--                    :template=\"'<span>#=kendo.toString(po, decimalFormat)#</span>'\"-->\n                <!--                    :headerAttributes=\"{-->\n                <!--                                                        style: 'background-color: #EDF1F5'-->\n                <!--                                                    }\"/>-->\n                <!--                <kendo-grid-column-->\n                <!--                    :field=\"'so'\"-->\n                <!--                    :title=\"$t('on_so')\"-->\n                <!--                    :width=\"150\"-->\n                <!--                    :attributes=\"{style: 'text-align: right; '}\"-->\n                <!--                    :template=\"'<span>#=kendo.toString(so, decimalFormat)#</span>'\"-->\n                <!--                    :headerAttributes=\"{-->\n                <!--                                                        style: 'background-color: #EDF1F5'-->\n                <!--                                                    }\"/>-->\n                <kendo-grid-column\n                    :field=\"'reOrderQty'\"\n                    :title=\"$t('reorder_qty')\"\n                    :width=\"150\"\n                    :attributes=\"{style: 'text-align: right; '}\"\n                    :template=\"'<span>#=kendo.toString(reOrderQty, decimalFormat)#</span>'\"\n                    :headerAttributes=\"{\n                                                        style: 'background-color: #EDF1F5'\n                                                    }\"/>\n                <kendo-grid-column\n                    :field=\"'qoh'\"\n                    :title=\"$t('qoh')\"\n                    :width=\"150\"\n                    :attributes=\"{style: 'text-align: right; '}\"\n                    :template=\"'<span>#=kendo.toString(qoh, decimalFormat)#</span>'\"\n                    :headerAttributes=\"{\n                                                        style: 'background-color: #EDF1F5'\n                                                    }\"/>\n                <kendo-grid-column\n                    :field=\"'wac'\"\n                    :title=\"$t('wac')\"\n                    :width=\"150\"\n                    :attributes=\"{style: 'text-align: right; '}\"\n                    :template=\"'<span>#=kendo.toString(wac, decimalFormat)#</span>'\"\n                    :headerAttributes=\"{\n                                                        style: 'background-color: #EDF1F5'\n                                                    }\"/>\n                <kendo-grid-column\n                    :field=\"'bValue'\"\n                    :title=\"$t('value')\"\n                    :width=\"150\"\n                    :attributes=\"{style: 'text-align: right; '}\"\n                    :template=\"'<span>#=kendo.toString(bValue, decimalFormat)#</span>'\"\n                    :headerAttributes=\"{\n                                                        style: 'background-color: #EDF1F5'\n                                                    }\"/>\n\n              </kendo-grid>\n            </template>\n          </v-col>\n        </v-row>\n      </v-card>\n    </v-col>\n  </v-row>\n</template>\n\n<script>\nimport DatePickerComponent from '@/components/custom_templates/DatePickerComponent'\n\nconst billingHandler = require(\"@/scripts/invoice/handler/billingHandler\")\n\nexport default {\n  data: () => ({\n    asOf: new Date().toISOString().substr(0, 10),\n    showLoading: false,\n    group: {field: 'name'},\n    receivableList: []\n  }),\n  components: {\n    LoadingMe: () => import(`@/components/Loading`),\n    'app-datepicker': DatePickerComponent\n  },\n  methods: {\n    async searchTransaction() {\n      new Promise(resolve => {\n        setTimeout(() => {\n          resolve('resolved')\n          this.showLoading = true\n          const startDate = this.asOf\n          let strFilter = '?asOf=' + startDate + '&reOrder=true'\n          billingHandler.inventoryBalance(strFilter).then(res => {\n            if (res.data.statusCode === 200) {\n              this.receivableList = res.data.data\n            }\n            this.showLoading = false\n          })\n\n        }, 10)\n      })\n    },\n    status(dataItem) {\n      let status = ''\n      switch (dataItem.status) {\n        case 1:\n          status = 'Open'\n          break\n        case 2:\n          status = 'Partially Paid'\n          break\n        case 3:\n          status = 'Paid'\n          break\n        case 4:\n          status = 'Void'\n          break\n      }\n      return status\n    },\n  },\n  computed: {},\n  mounted: async function () {\n    // await this.searchTransaction()\n  }\n};\n</script>\n<style scoped>\n.theme--light.v-data-table {\n  background-color: transparent !important;\n}\n\n.v-data-table > .v-data-table__wrapper > table > tbody > tr > td {\n  height: 32px !important;\n  border-bottom: thin solid rgba(0, 0, 0, 0.12) !important;\n}\n\n.v-data-table > .v-data-table__wrapper > table > tbody > tr:first-child > td {\n  border-top: thin solid rgba(0, 0, 0, 0.12) !important;\n}\n\n.theme--light.v-data-table > .v-data-table__wrapper > table > tbody > tr:hover:not(.v-data-table__expanded__content):not(.v-data-table__empty-wrapper) {\n  background-color: transparent !important;\n}\n\n@media (max-width: 576px) {\n  .paddingTopB {\n    padding-top: 0 !important;\n    padding-bottom: 0 !important;\n  }\n}\n</style>"],"sourceRoot":"src/views/products/product/stock_report"}]}