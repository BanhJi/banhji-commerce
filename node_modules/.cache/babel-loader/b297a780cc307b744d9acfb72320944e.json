       this.$emit('emitParentAccount', this.parentAccount);\n        },\n        onChanged() {\n            this.$emit('onChanged');\n        },\n    },\n    watch: {\n        initParentAccount: \"initialData\",\n        initAccountTypeCode() {\n            this.parentAccounts = store.state.accounting.accounts.filter((i) => i.type_code === this.initAccountTypeCode);\n        },\n    },\n    created() {\n        this.initialize();\n    },\n    mounted() {\n        /* Initial Obj */\n        this.initialData();\n    }\n}\n</script>"],"sourceRoot":"src/components/dropdownlist"}]}