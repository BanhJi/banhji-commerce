text-field\n                                        class=\"mt-1\"\n                                        outlined\n                                        :rules=\"[\n                      (v) => !!v || $t('name_is_required'),\n                      (v) => (v && v.length > 2) || $t('character_3'),\n                    ]\"\n                                        required\n                                        placeholder=\"\"\n                                        v-model=\"subGroup.name\"\n                                    />\n                                </v-col>\n                                <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                                    <v-row>\n                                        <v-col sm=\"4\" cols=\"4\" class=\"pt-0\">\n                                            <label class=\"label\">{{ $t(\"abbreviation\") }}</label>\n                                            <v-text-field\n                                                class=\"mt-1\"\n                                                outlined\n                                                tage=\"Abbreviation\"\n                                                :rules=\"[(v) => !!v || $t('abb_is_required')]\"\n                                                required\n                                                placeholder=\"\"\n                                                v-model=\"subGroup.abbr\"\n                                            />\n                                        </v-col>\n                                        <v-col sm=\"8\" cols=\"8\" class=\"py-0\">\n                                            <label class=\"label\">{{ $t(\"group\") }}</label>\n                                            <v-select\n                                                :loading=\"loading\"\n                                                class=\"mt-1\"\n                                                tag=\"group\"\n                                                v-model=\"mGroup\"\n                                                :items=\"groups\"\n                                                item-value=\"id\"\n                                                :item-text=\"(item) => `${item.abbr} - ${item.name}`\"\n                                                :rules=\"[(v) => !!v || $t('type_is_required')]\"\n                                                required\n                                                return-object\n                                                outlined\n                                            />\n                                        </v-col>\n                                    </v-row>\n                                </v-col>\n                                <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                                    <label class=\"label\">{{ $t(\"color\") }}</label>\n                                    <v-row>\n                                        <v-col class=\"py-1\">\n                                            <kendo-colorpalette\n                                                class=\"mt-1\"\n                                                v-model=\"colorPick\"\n                                                id=\"colorPickSubGroupDS\"\n                                                :palette=\"palette\"\n                                                :change=\"onGetColor\"\n                                                :tile-size=\"40\"\n                                            />\n                                        </v-col>\n                                    </v-row>\n                                </v-col>\n                                <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                                    <h2 class=\"mb-0 font_20 width_100\">\n                                        {{ $t(\"default_image\") }}\n                                    </h2>\n                                    <v-row>\n