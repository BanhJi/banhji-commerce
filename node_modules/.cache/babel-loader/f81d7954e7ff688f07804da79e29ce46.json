AA;AAAA;AAAA;AAOA,KAvBA;AAwBA,IAAA,UAxBA,wBAwBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,WAAA,CAAA,UAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,KAAA,GAAA,GAAA;AACA,iBAFA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KA5BA;AA6BA,IAAA,SAAA,EAAA,mBAAA,CAAA,EAAA;AACA,UAAA,IAAA,GAAA,KAAA,CAAA,MAAA,CAAA,iBAAA,EAAA,IAAA,CAAA,WAAA,CAAA;AACA,UAAA,KAAA,GAAA,CAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAFA,CAGA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,YAAA;AACA,YAAA,QAAA,GAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA;AACA,QAAA,CAAA,CAAA,kBAAA,QAAA,CAAA,GAAA,GAAA,IAAA,CAAA,CAAA,IAAA,CAAA,WAAA,EAAA,IAAA,CAAA,YAAA;AACA,cAAA,QAAA,CAAA,MAAA,CAAA,EAAA,KAAA,CAAA,EAAA;AACA,YAAA,CAAA,CAAA,IAAA,CAAA,CAAA,WAAA,CAAA,kBAAA;AACA;AACA,SAJA;AAKA,OAPA;AAQA,KAzCA;AA0CA,IAAA,WA1CA,uBA0CA,CA1CA,EA0CA;AAAA;;AACA,MAAA,CAAA,CAAA,cAAA;AACA,UAAA,IAAA,GAAA,KAAA,CAAA,MAAA,CAAA,OAAA,EAAA,IAAA,CAAA,WAAA,CAAA;AACA,UAAA,QAAA,GAAA,IAAA,CAAA,QAAA,CAAA,CAAA,CAAA,CAAA,CAAA,aAAA,CAAA,CAAA,OAAA,CAAA,IAAA,CAAA,CAAA;AACA,MAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,IAAA,GAAA,QAAA;AACA,WAAA,IAAA,CAAA,WAAA,GAAA,KAAA,IAAA,CAAA,QAAA;AACA,WAAA,IAAA,CAAA,aAAA,GAAA,IAAA,IAAA,GAAA,WAAA,GAAA,MAAA,CAAA,CAAA,EAAA,EAAA,CAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,MAAA,eAAA,CAAA,MAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAFA;AAGA,MAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,KAAA,IAAA,EAAA,MAAA;AACA;AAvDA,GA7DA;AAsHA,EAAA,OAtHA,qBAsHA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,MAAA,CAAA,UAAA,EADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,GAxHA;AAyHA,EAAA,QAAA,EAAA;AAzHA,CAAA","sourcesContent":["<template>\n    <v-row>\n        <v-col sm=\"12\" cols=\"12\" class=\"pt-0\">\n            <v-card color=\"white\" class=\"pl-2 no_border\" elevation=\"0\">\n                <v-row>\n                    <v-col cols=\"9\" class=\"py-0\">\n                        <h2 class=\"mb-0 font_20\">{{ $t('tasks_management') }}</h2>\n                        <p class=\"mb-0\">{{ $t('task_management_desc') }}</p>\n                    </v-col>\n                    <!-- <v-col cols=\"3\" class=\"\">\n                        <v-btn to=\"task\" color=\"primary\"\n                               class=\"dropdown_transactions white--text float-right text-capitalize\">\n                            {{ $t('new_task') }}\n                        </v-btn>\n                    </v-col> -->\n                </v-row>\n                <!-- loading -->\n                <LoadingMe\n                    :isLoading=\"compeletLoading\"\n                    :fullPage=\"false\"\n                    :myLoading=\"true\">\n\n                </LoadingMe>\n\n                <v-row class=\"\">\n                    <v-col sm=\"4\" cols=\"12\" class=\"pb-0\">\n                        <v-select class=\"mt-1\"\n                                  :items=\"dateSorters\"\n                                  item-value=\"id\"\n                                  item-text=\"name\"\n                                  v-model=\"dataSorter\"\n                                  return-object\n                                  clearable\n                                  outlined\n                                  placeholder=\"ALL\"\n                        />\n                    </v-col>\n\n<!--                    <v-col sm=\"3\" cols=\"12\" class=\"pb-0\">-->\n<!--                        <app-datepicker :initialDate=\"start_date\" @emitDate=\"start_date = $event\"/>-->\n<!--                    </v-col>-->\n\n<!--                    <v-col sm=\"3\" cols=\"12\" class=\"pb-0\">-->\n<!--                        <app-datepicker :initialDate=\"end_date\" @emitDate=\"end_date = $event\"/>-->\n<!--                    </v-col>-->\n\n                    <v-col sm=\"2\" cols=\"1\" class=\"pb-0 pt-4\">\n\n                        <v-btn @click=\"searchTaskByFrequency\" color=\"primary white--text\">\n                            <i class=\"b-search\" style=\"font-size: 18px; color:#fff !important;\"/>\n                        </v-btn>\n                    </v-col>\n                </v-row>\n                <v-dialog v-model=\"dialog\" max-width=\"450px\">\n                    <v-card>\n                        <div class=\"modal_header\">\n                            <v-card-title>{{ task.name }}</v-card-title>\n                            <v-icon class=\"btn_close\" @click=\"cancel\">close</v-icon>\n                        </div>\n                        <v-card-text class=\"modal_text_content\">\n                            <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n                                <v-row>\n                                    <v-col sm=\"12\" cols=\"12\" class=\"\">\n                                        <label class=\"label\">{{ $t(\"completed_by\") }}</label>\n                                        <v-autocomplete\n                                            v-model=\"task.completedBy\"\n                                            :items=\"employees\"\n                                            item-value=\"id\"\n                                            item-text=\"name\"\n                                            :rules=\"[v => !!v || 'employee is required']\"\n                                            outlined\n                                            return-object\n                                            clearable\n                                            class=\"\"\n                                        />\n                                    </v-col>\n                                    <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                                        <label class=\"label\">{{ $t(\"completed_date\") }}</label>\n                                        <app-datepicker :initialDate=\"task.completedDate\" @emitDate=\"task.completedDate = $event\"/>\n                                    </v-col>\n                                    <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                                        <label class=\"label\">{{ $t(\"reference\") }}</label>\n                                        <v-text-field v-model=\"task.reference\" outlined/>\n                                    </v-col>\n                                </v-row>\n                            </v-form>\n                        </v-card-text>\n                        <v-divider/>\n                        <v-card-actions class=\"modal_footer\">\n                            <v-btn class=\"btn_cancel\">{{ $t(\"cancel\") }}</v-btn>\n                            <v-btn @click=\"completed\" class=\"btn_save_close\"\n                            >{{ $t(\"save_close\") }}\n                            </v-btn>\n                        </v-card-actions>\n                    </v-card>\n                </v-dialog>\n                <v-row>\n                    <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                        <div class=\"reports_table\">\n                            <kendo-datasource ref=\"dataSource\"\n                                              :data=\"tasks\"\n                                              :schema=\"gridSchema\"\n                                              :server-paging=\"false\"\n                                              :group=\"groupDefinition\"/>\n                            <kendo-grid id=\"grid\"\n                                        class=\"grid-function\"\n                                        :data-source-ref=\"'dataSource'\"\n                                        :style=\"{width: '100%'}\"\n                                        :noRecords=\"true\"\n                                        :groupable=\"true\"\n                                        :column-menu=\"true\"\n                                        :pageable-numeric=\"false\"\n                                        :pageable-previous-next=\"false\"\n                                        :pageable-messages-display=\"'Showing {2} data items'\"\n                                        :scrollable-virtual=\"true\">\n\n                                <kendo-grid-column\n                                    :field=\"'name'\"\n                                    :title=\"$t('name')\"\n                                    :attributes=\"{class:'tb_name_td'}\"\n                                    :width=\"100\"\n                                    :template=\"'<span>#=name#</span>'\"\n                                    :groupHeaderColumnTemplate=\"'#=value#'\"\n                                    :headerAttributes=\"{ style: 'background-color: #EDF1F5' }\"/>\n                                <kendo-grid-column\n                                    :field=\"'category.name'\"\n                                    :title=\"$t('categories')\"\n                                    :template=\"'<span>#=category.name#</span>'\"\n                                    :width=\"100\"\n                                    :headerAttributes=\"{ style: 'background-color: #EDF1F5' }\"/>\n                                <kendo-grid-column\n                                    :field=\"'type.name'\"\n                                    :title=\"$t('task_type')\"\n                                    :template=\"'<span>#=type.name#</span>'\"\n                                    :width=\"80\"\n                                    :headerAttributes=\"{style: 'text-align: left; background-color: #EDF1F5'}\"/>\n                                <kendo-grid-column\n                                    :field=\"'subType.name'\"\n                                    :template=\"'<span>#=subType.name#</span>'\"\n                                    :title=\"$t('sub_type')\"\n                                    :width=\"80\"\n                                    :headerAttributes=\"{style: 'text-align: left; background-color: #EDF1F5'}\"/>\n                                <kendo-grid-column\n                                    :field=\"'dueDate'\"\n                                    :title=\"$t('due_date')\"\n                                    :width=\"90\"\n                                    :headerAttributes=\"{style: 'text-align: left; background-color: #EDF1F5'}\"/>\n                                <kendo-grid-column\n                                    :field=\"'frequency.name'\"\n                                    :title=\"$t('frequency')\"\n                                    :width=\"80\"\n                                    :template=\"'<span>#=frequency.name#</span>'\"\n                                    :headerAttributes=\"{style: 'text-align: left; background-color: #EDF1F5'}\"/>\n                                <kendo-grid-column\n                                    :field=\"'reference'\"\n                                    :title=\"$t('ref')\"\n                                    :width=\"80\"\n                                    :headerAttributes=\"{style: 'text-align: left; background-color: #EDF1F5'}\"/>\n                                <kendo-grid-column\n                                    :field=\"'status.name'\"\n                                    :title=\"$t('status')\"\n                                    :width=\"80\"\n                                    :template=\"'<span>#=status.name#</span>'\"\n                                    :headerAttributes=\"{style: 'text-align: left; background-color: #EDF1F5'}\"/>\n                                <kendo-grid-column\n                                    :field=\"''\"\n                                    :title=\"$t('action')\"\n                                    :width=\"70\"\n                                    v-on:databound=\"dataBound\"\n                                    :command=\"[{iconClass: 'k-icon k-i-edit', text: 'Completed', click: goCompleted , className: 'k-state-disabled isStatus' }]\"\n                                    :headerAttributes=\"{style: 'text-align: right; background-color: #EDF1F5'}\"/>\n                            </kendo-grid>\n                        </div>\n                    </v-col>\n                </v-row>\n            </v-card>\n        </v-col>\n    </v-row>\n</template>\n\n<script>\nimport LoadingMe from '@/components/Loading'\nimport DatePickerComponent from '@/components/custom_templates/DatePickerComponent'\nimport Task from \"@/scripts/model/Task\";\nconst taskHandler = require(\"@/scripts/taskHandler\")\nconst employeeHandler = require(\"@/scripts/employeeHandler\");\nconst $ = require('jquery')\nimport kendo from \"@progress/kendo-ui\";\nexport default {\n    name: \"\",\n    components: {\n        LoadingMe,\n        'app-datepicker': DatePickerComponent,\n        // BankConnection,\n    },\n    watch: {\n        // async searchCompany() {\n        //     // Items have already been loaded\n        //     if (this.employees.length > 0) return\n        //\n        //     this.isLoading = true\n        //     // Lazily load input items\n        //     fetch(await employeeHandler.search())\n        //         .then(res => res.clone().json())\n        //         .then(res => {\n        //             this.employees = res.data\n        //         })\n        //         .catch(err => {\n        //             window.console.log(err)\n        //         })\n        //         .finally(() => (this.isLoading = false))\n        // },\n    },\n    data: () => ({\n        start_date: \"\",\n        end_date: \"\",\n        dateSorters: [\n            {id: 1, name: 'Daily'},\n            {id: 2, name: 'Weekly'},\n            {id: 3, name: 'Monthly'},\n            {id: 4, name: 'Quarterly'},\n            {id: 5, name: 'Annually'}\n        ],\n        dataSorter: {},\n        journal_entries: [],\n        // LoadingMe\n        compeletLoading: false,\n        isLoaded: false,\n        tasks: [],\n        schemaDefinition: {\n            model: {id: \"id\"}\n        },\n        /* Sort by account number */\n        sortDefinition: {field: \"number\"},\n        gridSchema: {\n            model: {\n                id: \"id\"\n            }\n        },\n        groupDefinition: {\n            field: \"name\",\n        },\n        search: '',\n        categories: [],\n        dialog: false,\n        valid: true,\n        task: new Task({}),\n        employees: [],\n    }),\n    methods: {\n        cancel() {\n            this.dialog = false\n            this.task = new Task({})\n        },\n        async searchTaskByFrequency() {\n            if (this.dataSorter !== undefined) {\n                await taskHandler.getTaskByFrequency(this.dataSorter.id).then(res => {\n                    this.tasks = res\n                    window.console.log(res)\n                })\n            } else {\n                await this.onloadTask()\n            }\n\n        },\n        async completed() {\n            this.task.status = {id: 2, name: 'Completed'}\n            window.console.log(this.task)\n            await taskHandler.saveTask(this.task).then(() => {\n                this.dialog = false\n                this.onloadTask()\n            })\n        },\n        async onloadTask() {\n            await taskHandler.getAllTask().then(res => {\n                this.tasks = res\n            })\n        },\n        dataBound: function (e) {\n            const grid = kendo.jQuery(\"#gridServiceUOM\").data(\"kendoGrid\")\n            const items = e.sender.items()\n            // window.console.log(items)\n            items.each(function () {\n                let dataItem = grid.dataItem(this);\n                $(\"tr[data-uid='\" + dataItem.uid + \"']\").find(\".isStatus\").each(function () {\n                    if (dataItem.status.id === 1) {\n                        $(this).removeClass('k-state-disabled')\n                    }\n                })\n            })\n        },\n        goCompleted(e) {\n            e.preventDefault();\n            let grid = kendo.jQuery(\"#grid\").data(\"kendoGrid\");\n            let dataItem = grid.dataItem($(e.currentTarget).closest(\"tr\"));\n            window.console.log(dataItem)\n            this.task = dataItem\n            this.task.completedBy = this.task.assignTo\n            this.task.completedDate = new Date().toISOString().substr(0, 10)\n            this.dialog = true\n            employeeHandler.getAll().then(res => {\n                this.employees = res.data.data\n            })\n            window.console.log(this.task, 'task')\n        },\n    },\n    async mounted() {\n        await this.onloadTask()\n    },\n    computed: {},\n};\n</script>\n<style scoped>\n</style>\n"],"sourceRoot":"src/views/compliance/task"}]}