ass=\"btn_close\" @click=\"dialogm3 = false\">close</v-icon>\n                            <v-card-text\n                                style=\"height: 200px; background-color: #EDF1F5; color: #333;\"\n                            >\n                                <v-row>\n                                    <v-col sm=\"6\" cols=\"12\" class=\"pb-0\">\n                                        <label>{{ $t(\"batch_id\") }}</label>\n                                        <v-text-field\n                                            v-model=\"batch.batchId\"\n                                            class=\" my-2\"\n                                            tage=\"Batch Id\"\n                                            outlined\n                                            clearable\n                                        />\n                                    </v-col>\n                                    <v-col sm=\"6\" cols=\"12\" class=\"pb-0\">\n                                        <label class=\"label\">{{ $t(\"expiration_date\") }}</label>\n                                        <app-datepicker :initialDate=\"batch.expireDate\" @emitDate=\"batch.expireDate = $event\" />\n                                    </v-col>\n                                    <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                                        <label>{{ $t(\"descriptions\") }}</label>\n                                        <v-text-field\n                                            class=\" my-2\"\n                                            v-model=\"batch.description\"\n                                            outlined\n                                            tage=\"Descriptions\"\n                                            clearable\n                                        />\n                                    </v-col>\n                                </v-row>\n                            </v-card-text>\n                            <v-card-actions class=\"pa-5\">\n                                <v-row>\n                                    <v-col sm=\"6\" cols=\"6\" class=\"py-0 text-left\">\n                                        <v-btn\n                                            color=\"black\"\n                                            outlined\n                                            class=\" text-capitalize  black--text float-left\"\n                                            @click=\"dialogm2 = false\"\n                                        >{{ $t(\"cancel\") }}\n                                        </v-btn\n                                        >\n                                    </v-col>\n                                    <v-col sm=\"6\" cols=\"6\" class=\"py-0 text-right\">\n                                        <v-btn\n                                            @click=\"onCreateBatch()\"\n                                            color=\"primary\"\n                                            class=\"px-3  white--text text-capitalize\"\n                                        >{{ $t(\"save_close\") }}\n                                        </v-btn>\n                                    </v-col>\n                                </v-row>\n                            </v-card-actions>\n                        </v-card>\n                    </v-dialog>\n                </v-col>\n            </v-row>\n            <v-row>\n                <v-col sm=\"9\" cols=\"12\" class=\"py-0\">\n                    <v-row class=\"\">\n                        <v-col sm=\"4\" cols=\"12\" class=\"py-0\">\n                            <v-select\n                                class=\"mt-1\"\n                                :items=\"dateSorters\"\n                                clearable\n                                outlined\n                                placeholder=\"ALL\"\n                            />\n                        </v-col>\n\n                        <v-col sm=\"3\" cols=\"12\" class=\"py-0\">\n                            <app-datepicker\n                                :initialDate=\"start_date\"\n                                @emitDate=\"start_date = $event\"\n                            />\n                        </v-col>\n\n                        <v-col sm=\"3\" cols=\"12\" class=\"py-0\">\n                            <app-datepicker\n                                :initialDate=\"end_date\"\n                                @emitDate=\"end_date = $event\"\n                            />\n                        </v-col>\n\n                        <v-col sm=\"1\" cols=\"12\" class=\"py-1\">\n                            <v-btn\n                                class=\"btn_search\"\n                                style=\"background-color: rgb(237, 241, 245)\"\n                            >\n                                <i class=\"b-search\" style=\"font-size: 18px; color:#fff !important;\"/>\n                            </v-btn>\n                        </v-col>\n                    </v-row>\n                </v-col>\n            </v-row>\n\n            <v-row class=\"\">\n                <v-col sm=\"12\" cols=\"12\" class=\"py-0\">\n                    <template>\n                        <v-simple-table class=\"attachment_table\">\n                            <template v-slot:default>\n                                <thead>\n                                <tr>\n                                    <th>{{ $t(\"batch_id\") }}</th>\n                                    <th>{{ $t(\"expiration_date\") }}</th>\n                                    <th>{{ $t(\"stock\") }}</th>\n                                </tr>\n                                </thead>\n                                <tbody v-for=\"batch in batchs\" v-bind:key=\"batch.id\">\n                                <tr>\n                                    <td>{{ batch.batchId }}</td>\n                                    <td>{{ batch.expireDate }}</td>\n                                    <td>{{ batch.stock }}</td>\n                                </tr>\n                                </tbody>\n                            </template>\n                        </v-simple-table>\n                    </template>\n                </v-col>\n            </v-row>\n        </v-col>\n    </v-row>\n</template>\n<script>\nimport {BatchModel} from \"@/scripts/model/AppModels\";\n\nconst batchHandler = require('@/scripts/batchHandler')\nexport default {\n    data: () => ({\n        // Search transaction dates\n        start_date: \"\",\n        end_date: \"\",\n        dateSorters: [\"Today\", \"This Week\", \"This Month\", \"This Year\"],\n        account: [],\n        files: [],\n        attachments: [],\n\n        dialogm3: false,\n        // LoadingMe\n        isLoaded: false,\n        // Kendo dataSource\n        schemaDefinition: {\n            model: {\n                id: \"id\",\n            },\n        },\n        /* Sort by account number */\n        sortDefinition: {\n            field: \"number\",\n        },\n        batch: new BatchModel({}),\n        batchs: []\n    }),\n    props: {},\n    methods: {\n        onCreateBatch() {\n            window.console.log(this.batchs)\n            batchHandler.save(this.batch).then(res => {\n                if (res.data.statusCode === 201) {\n                    this.onLoadBatch()\n                    this.dialogm3 = false\n                }\n            })\n        },\n        async onLoadBatch() {\n            batchHandler.getAll().then(res => {\n                this.batchs = res.data.data\n            })\n        }\n    },\n    async mounted() {\n        await this.onLoadBatch()\n    },\n    computed: {},\n    components: {\n        \"app-datepicker\": () => import('@/components/custom_templates/DatePickerComponent'),\n    },\n};\n</script>\n\n<style scoped>\n.function_footer {\n    padding: 15px;\n    display: inline-block;\n}\n\np {\n    color: rgba(0, 0, 0, 0.87);\n}\n\n.actionBtn {\n    height: 47.2px !important;\n}\n\n.text_tip {\n    font-size: 9px;\n    line-height: 10px;\n}\n\n@media (max-width: 576px) {\n}\n</style>\n"],"sourceRoot":"src/views/products/warehouses"}]}