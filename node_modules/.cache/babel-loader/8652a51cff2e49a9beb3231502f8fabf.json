    :scrollable-virtual=\"true\"\n                            >\n                                <kendo-grid-column\n                                    :field=\"'name'\"\n                                    :attributes=\"{class:'tb_name_td'}\"\n                                    :title=\"$t('name')\"\n                                    :template=\"'<span>#=name#</span>'\"\n                                    :headerAttributes=\"{\n                                                        style: 'background-color: #EDF1F5'\n                                                    }\"/>\n                                <kendo-grid-column\n                                    :field=\"'amount'\"\n                                    :title=\"$t('amount')\"\n                                    :template=\"'<span>#=amount#</span>'\"\n                                    :headerAttributes=\"{\n                                                        style: 'background-color: #EDF1F5'\n                                                    }\"/>\n                                <kendo-grid-column\n                                    :field=\"'paymentTerm'\"\n                                    :title=\"$t('term')\"\n                                    :template=\"'<span>#=paymentTerm.name#</span>'\"\n                                    :headerAttributes=\"{\n                                                        style: 'background-color: #EDF1F5'\n                                                    }\"/>\n                                <kendo-grid-column\n                                    :field=\"'aging'\"\n                                    :title=\"$t('aging')\"\n                                    :template=\"'<span>#=overDue#</span>'\"\n                                    :headerAttributes=\"{\n                                                        style: 'background-color: #EDF1F5'\n                                                    }\"/>\n                               